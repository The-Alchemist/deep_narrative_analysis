:Glucocorticoid a owl:Class ;
  rdfs:subClassOf :Corticosteroid, :Vasoconstrictor ;
  :definition "Medication providing anti-inflammatory, immunosuppressive, anti-proliferative, and vasoconstrictive effects."@en ;
  :example "cortisol"@en .

:GonadotropicHormoneAndAgonist a owl:Class ;
  rdfs:subClassOf :HormoneAndAgonist, :InfertilityTreatment ;
