:Fluoroquinolone a owl:Class ;
  rdfs:subClassOf :Antibiotic ;
  :definition "Antibiotic used to treat illnesses such as respiratory and urinary tract infections."@en .

:FollicleStimulatingHormone a owl:Class ;
  rdfs:subClassOf :GonadotropicHormoneAndAgonist ;
  :synonym "FSH" .

:Galactofuge a owl:Class ;
  rdfs:subClassOf :PharmaceuticalAndMedicinal ;
  :definition "Substance that stops or reduces the flow of milk in a nursing mother."@en .

:Galactogogue a owl:Class ;
  rdfs:subClassOf :PharmaceuticalAndMedicinal ;
  :definition "Substance that promotes lactation in a nursing mother."@en ;
