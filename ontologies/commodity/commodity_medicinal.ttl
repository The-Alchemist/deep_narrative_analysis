:ErythropoietinAndAgonist a owl:Class ;
  rdfs:subClassOf :HormoneAndAgonist ;
  :definition "Hormone produced primarily by the kidneys which aid in the production of red blood cells."@en ;
  :synonym "erythropoietin"@en, "erythropoietin agonist"@en, "erythropoietin receptor agonist"@en, "EPoR agonist"@en .

:EstrogenAgonist a owl:Class ;
  rdfs:subClassOf :HormoneAndAgonist, :Antiandrogenic ;
