@prefix : <urn:ontoinsights:ontology:dna:> .
@prefix dna: <urn:ontoinsights:ontology:dna:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

########################################################################
# File defining the concept of a flowering plant and its parts, along
#    with properties (such as hardiness, edibility, etc.)
# Also, the file defines the (scientific) families of flowering plants 
#    that are cultivated and sold as commodities 
#    (these are found at the bottom)
# The classes reference entities in codes 06, 07, 08 and 10 of the UN
#    Commodities list 
#    (https://comtrade.un.org/db/mr/rfCommoditiesList.aspx)
# 
# Created: February 13, 2020
# Last modified: February 13, 2020
########################################################################


########################################################################
# Classes
########################################################################

:FloweringPlant a owl:Class ;
  rdfs:subClassOf :Plant ;
  :definition "A type of Plant that produces flowers, fruits and seeds (within the fruits). The group includes herbaceous (non-woody) plants, trees, shrubs and grasses."@en ;
  :synonym "angiosperm"@en .

:PlantPart a owl:Class ;
  :definition "An enumeration specifying the various parts of a plant, such as the flower or root."@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :PlantBud :PlantLeaf :PlantPollen :PlantSeedpod :PlantFlower :PlantRoot :PlantShoot :PlantFruit
                                    :PlantNectar :PlantSapAndManna :PlantStem :PlantInnerBark :PlantOil :PlantSeed ) ] .


########################################################################
# Datatype Properties
########################################################################

:plant_country_range a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "String specifying the continents/countries where the plant is typically found."@en ;
  rdfs:range xsd:string .

:plant_edibility_rating a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "An integer indicating the plant's value as a food source, where 1 indicates minor value and 5 indicates large value."@en ;
  rdfs:range xsd:integer .

:plant_edible_text a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "String providing details for the :plant_edibility_rating."@en ;
  rdfs:range xsd:string .

:plant_habitat a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "String specifying the environmental conditions where the plant grows or can be cultivated."@en ;
  rdfs:range xsd:string .

:plant_hardiness_zones a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "String specifying the USDA hardiness zones where the plant is typically grown. Zones are numbered from 1 to 13, where zone 1 is the coldest and zone 13 is the warmest, defined based on the average, annual minimum winter temperature."@en ;
  rdfs:comment "Zones are defined in 10 degree F increments where zone 1 is the range -60 to -50 F, zone 2 is the range -50 to -40 F, ... and zone 13 is the range 60 to 70 F. Therefore, if a plant is described with a USDA hardiness of '4-8', it should be able to withstand a minimum temperature of -30 F, and will also do well up to zone 8."@en ;
  rdfs:range xsd:string .

:plant_hazards a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "String specifying any dangers or risks associated with the plant."@en ;
  rdfs:range xsd:string .

:plant_medicinal_rating a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "An integer indicating the plant's medicinal value, where 1 indicates minor value and 5 indicates large value."@en ;
  rdfs:range xsd:integer .

:plant_medicinal_text a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "String providing details for the :plant_medicinal_rating."@en ;
  rdfs:range xsd:string .

:plant_other_use_rating a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "An integer indicating the plant's usefulness other than medicinally or as a food source, where 1 indicates minor value and 5 indicates large value."@en ;
  rdfs:range xsd:integer .

:plant_other_use_text a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "String providing details for the :plant_other_use_rating."@en ;
  rdfs:range xsd:string .

:plant_physical_characteristics a owl:DatatypeProperty, owl:FunctionalProperty ;
  :definition "String describing whether the plant is a tree, shrub, climber, etc., when it flowers and other physical features and qualities."@en ;
  rdfs:range xsd:string .


########################################################################
# Object Properties
########################################################################

:plant_edible_part a owl:ObjectProperty ;
  :definition "Reference to the parts of the plant that are edible, such as the flowers or root."@en ;
  rdfs:range :PlantPart .

:plant_edible_use a owl:ObjectProperty ;
  :definition "Reference to the edible uses of a plant, for example, use as a sweetener or to make tea."@en ;
  rdfs:range :Food .

:plant_medicinal_use a owl:ObjectProperty ;
  :definition "Reference to the medicinal uses of a plant, for example, use as an antacid or as an astringent."@en ;
  rdfs:range :PharmaceuticalAndMedicinal .

:plant_other_use a owl:ObjectProperty ;
  :definition "Reference to 'other' uses of a plant beyond as a food source or for medicinal purposes, for example, use as bedding or for soap making."@en ;
  rdfs:range :ToolAndCommodity .


########################################################################
# Individuals
########################################################################

:PlantBud a :PlantPart ;
  :definition "From Wikipedia (https://en.wikipedia.org/w/index.php?title=Bud&oldid=935131142), a Bud 'is an undeveloped or embryonic shoot and normally occurs in the axil of a leaf or at the tip of a stem. Once formed, a bud may remain for some time in a dormant condition, or it may form a shoot immediately. Buds may be specialized to develop flowers or short shoots, or may have the potential for general shoot development.'"@en .

:PlantFlower a :PlantPart ;
  :definition "The reproductive structures of angiosperms/flowering plants."@en ;
  :synonym "blossom"@en, "bud"@en, "floret"@en, "floweret"@en .

:PlantFruit a :PlantPart ;
  :definition "The seed-bearing structures of angiosperms/flowering plants."@en ;
  :synonym "drupe"@en, "drupelet"@en .

:PlantInnerBark a :PlantPart ;
  :definition "Bark is the outermost layers of the stems and roots of woody plants. It typcially lives for only a short time and then becomes part of the outer, protective bark layer."@en .

:PlantLeaf a :PlantPart ;
  :definition "A flat, typically green organ a plant, that is attached to a stem or stalk. Its use is for photosynthesis and transpiration."@en ;
  :synonym "frond"@en, "leaflet"@en, "leaves"@en .

:PlantNectar a :PlantPart ;
  :definition "A sugar-rich liquid secreted by flowers or via extrafloral nectaries. It is used to attract birds, insects, etc. that pollinate, protect or otherwise benefit the plant."@en .

:PlantPollen a :PlantPart ;
  :definition "A (fine to coarse) powdery substance which are discharged as grains from the male part of a flower or cone. The grains produce male gametes that can fertilize a female pistil or cone, when transported by wind, insects, or other animals."@en .

:PlantRoot a :PlantPart ;
  :definition "The part of a plant that attaches it to the ground or to a support, and typically lies below the surface of the ground. It absorbs water and other elements for the plant, and stores food and nutrients."@en ;
  :synonym "tuber"@en, "leaflet"@en, "leaves"@en .

:PlantSapAndManna a :PlantPart ;
  :definition "The fluid that circulates through a plant's vascular system (xylem and phloem). It is mostly water with hormones and minerals/nutrients. The phloem sap also contains sugars. When the sap is crystallized, that is referred to as manna."@en ;
  :synonym "frond"@en, "leaflet"@en, "leaves"@en .

:PlantSeed a :PlantPart ;
  :definition "A plant's reproductive unit which consists of an embryonic plant, a source of nutrients for it and a protective covering (seed coat or seedpod)."@en .

:PlantSeedpod a :PlantPart ;
  :definition "A seed's protective covering."@en ;
  :synonym "seed vessel"@en, "seed coat"@en, "seedcase"@en .

:PlantShoot a :PlantPart ;
  :definition "From Wikipedia (https://en.wikipedia.org/w/index.php?title=Shoot&oldid=899711436), a shoot 'consist of stems including their appendages, the leaves and lateral buds, flowering stems and flower buds. The new growth from seed germination that grows upward is a shoot where leaves will develop. In the spring, perennial plant shoots are the new growth that grows from the ground in herbaceous plants or the new stem or flower growth that grows on woody plants.'"@en .

:PlantStem a :PlantPart ;
  :definition "The structural body/stalk of a plant. It consists of nodes (holding leaves and buds) and internodes (which separate nodes)."@en ;
  :synonym "branch"@en, "trunk"@en, "stalk"@en .


