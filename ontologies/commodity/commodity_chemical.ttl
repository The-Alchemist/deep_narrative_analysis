########################################################################
# Classes
########################################################################

:Acid a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A typically corrosive chemical that neutralizes alkalis, and turns litmus paper to a red color."@en ;
  rdfs:comment "UN Harmonized System codes 2806 (hydrogen chloride/hydrocloric acid and chlorosulphuric acid), 2807 (sulphuric acid/oleum), 2808 (nitric and sulphonitric acids), 2809 (phosphoric and polyphosphoric acids), 2810 (boric acid), 2811 (hydrogen fluoride/hydroflouric acid) and 2815 (sodium hydroxide/caustic soda and potassium hydroxide/caustic potash)"@en ;
  :synonym "corrosive"@en ;
  :example "hydrogen chloride"@en, "oleum"@en, "hydrogen fluoride"@en, "sodium hydroxide"@en, "potassium hydroxide"@en ;
  :negative_example "amino acid"@en .

:ActinideSeries a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A series in the periodic table with atomic numbers from 89 to 103 consisting of the elements, actinium (Ac), thorium (Th), protactinium (Pa), uranium (U), neptunium (Np), plutonium (Pu), americium (Am), curium (Cm), berkelium (Bk), californium (Cf), einsteinium (Es), fermium (Fm), mendelevium (Md), noblelium (No) and lawrencium (Lr)."@en ;
  rdfs:comment "UN Harmonized System code 2844"@en ;
  :synonym "actinide"@en, "actinoid"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Actinium :Thorium :Protactinium :Uranium :Neptunium :Plutonium :Americium :Curium :Berkelium :Californium 
                                    :Einsteinium :Fermium :Mendelevium :Noblelium :Lawrencium ) ] .

:AlkalineMetal a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 2) consisting of beryllium (Be), magnesium (Mg), calcium (Ca), strontium (Sr), barium (Ba) and radium (Ra)."@en ;
  rdfs:comment "UN Harmonized System codes 2805 (barium), 2844 (radium), 8104 (magnesium) and 8112 (beryllium)"@en ;
  :synonym "alkaline-earth metal"@en, "Group 2 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Beryllium :Magnesium :Calcium :Strontium :Barium :Radium ) ] .

:Alloy a owl:Class ;
  rdfs:subClassOf :ChemicalCompound ;
  :definition "From Wikipedia(https://en.wikipedia.org/w/index.php?title=Alloy&oldid=939624610), an Alloy 'is a combination of metals or metals combined with one or more other elements. For example, combining the metallic elements gold and copper produces red gold, gold and silver becomes white gold, and silver combined with copper produces sterling silver.'"@en ;
  rdfs:comment "UN Harmonized System codes 2844, 72-76 (alloys)"@en ;
  :synonym "corrosive"@en ;
  :example "hydrogen chloride"@en, "oleum"@en, "hydrogen fluoride"@en, "sodium hydroxide"@en, "potassium hydroxide"@en ;
  :negative_example "amino acid"@en .

:BoronGroup a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 13) also known as the 'triels' group, consisting of boron (B), aluminium (Al), gallium (Ga), indium (In), thallium (TI) and nihonium (Nh)."@en ;
  rdfs:comment "UN Harmonized System codes 2804 (boron), 2844 (nihonium) and 76 (aluminum/aluminium)"@en ;
  :synonym "triels"@en, "Group 13 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Boron :Aluminum :Aluminium :Gallium :Indium :Thallium :Nihonium ) ] .

:CarbonGroup a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 14) also known as the 'tetrels' group, consisting of carbon (C), silicon (Si), germanium (Ge), tin (Sn), lead (Pb) and flerovium (Fl)."@en ;
  rdfs:comment "UN Harmonized System codes 2803 (carbon), 2804 (silicon), 2824 (lead), 2844 (flerovium), 78 (lead), 80 (tin) and 8112 (germanium)"@en ;
  :synonym "tetrels"@en, "Group 14 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Carbon :Silicon :Germanium :Tin :Lead :Flerovium ) ] .

:ChemicalCompound a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "From Wikipedia (https://en.wikipedia.org/w/index.php?title=Chemical_compound&oldid=935156698), a ChemicalCompound 'is a chemical substance composed of many identical molecules (or molecular entities) composed of atoms from more than one element held together by chemical bonds. Two atoms of the same element bonded in a molecule do not form a chemical compound, since this would require two different elements. There are four types of compounds, depending on how the constituent atoms are held together: (1) molecules held together by covalent bonds; (2) ionic compounds held together by ionic bonds; (3) intermetallic compounds held together by metallic bonds; and, (4) certain complexes held together by coordinate covalent bonds.'"@en ;
  rdfs:comment "UN Harmonized System codes 2835, 2843-2846, 2850 and 2851 (ccompounds). Note that when instantiating a ChemicalCompound, the :has_component property can be used to reference the composing elements."@en .

:ChromiumGroup a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 6) consisting of chromium (Cr), molybdenum (Mo), tungsten or wolfram (W) and seaborgium (Sg)."@en ;
  rdfs:comment "UN Harmonized System codes 2844 (seaborgium), 8101 (tungsten/wolfram), 8102 (molybdenum) and 8112 (chromium)"@en ;
  :synonym "Group 6 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Chromium :Molybdenum :Tungsten :Wolfram :Seaborgium ) ] .

:CladdedMetal a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "From ThomasNet (https://www.thomasnet.com/articles/metals-metal-products/about-clad-metals/), 'clad metals are metals bonded to at least one layer of a different metal. Cladding can be achieved by a number of different processes, including extrusion, pressing, electroplating and a variety of chemical techniques. The advantages to cladding range from enhanced appearance and corrosion resistance to superior thermal and electrical performance, and the process is often used to protect less wear-resistant metals.'"@en ;
  rdfs:comment "UN Harmonized System codes 71-72 (metal clad)"@en ;
  :synonym "clad metal"@en .

:CobaltGroup a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 9) consisting of cobalt (Co), rhodium (Rh), iridium (Ir) and meitnerium (Mt)."@en ;
  rdfs:comment "UN Harmonized System codes 2844 (meitnerium) and 8105 (cobalt)"@en ;
  :synonym "Group 9 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Cobalt :Rhodium :Iridium :Meitnerium ) ] .

:CopperGroup a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 11) also known as the 'coinage metals', consisting of copper (Cu), silver (Ag), gold (Au) and roentgenium (Rg)."@en ;
  rdfs:comment "UN Harmonized System codes 2844 (roentgenium) and 74 (copper)"@en ;
  :synonym "coinage metal"@en, "Group 11 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Copper :Silver :Gold :Roentgenium ) ] .

:Chalcogen a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 16) consisting of oxygen (O), sulfur (S), selenium (Se), tellurium (Te), polonium (Po) and livermorium (Lv)."@en ;
  rdfs:comment "UN Harmonized System codes 2802 (sulfur), 2804 (tellurium and selenium) and 2844 (polonium and livermorium)"@en ;
  :synonym "Group 16 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Oxygen :Sulfur :Selenium :Tellurium :Polonium :Livermorium ) ] .

:Halogen a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 17) consisting of fluorine (F), chlorine (Cl), bromine (Br), iodine (I), astatine (At) and tennessine (Ts)."@en ;
  rdfs:comment "UN Harmonized System codes 2801 and 2844 (astatine)"@en ;
  :synonym "flourine group"@en, "Group 17 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Fluorine :Chlorine :Bromine :Iodine :Astatine :Tennessine ) ] .

:HydrogenAndAlkaliMetal a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 1) consisting of hydrogen (H), lithium (Li), sodium (Na), potassium (K), rubidium (Rb), caesium (Cs) and francium (Fr)."@en ;
  rdfs:comment "UN Harmonized System codes 2802 (sulfur), 2804 (hydrogen), 2805 and 2844 (caesium and francium)"@en ;
  :synonym "Group 1 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Hydrogen :Lithium :Sodium :Potassium :Rubidium :Caesium :Francium ) ] .

:IronGroup a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 8) consisting of iron (Fe), ruthenium (Ru), osmium (Os) and hassium (Hs)."@en ;
  rdfs:comment "UN Harmonized System codes 2844 (hassium) and 72 (iron)"@en ;
  :synonym "Group 8 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Iron :Ruthenium :Osmium :Hassium ) ] .

:LanthanideSeries a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A series in the periodic table with atomic numbers from 57-71, consisting of the elements, lanthanum (La), cerium (Ce), praseodymium (Pr), neodymium (Nd), promethium (Pm), samarium (Sm), europium (Eu), gadolinium (Gd), terbium (Tb), dysorisium (Dy), holmium (Ho), erbium (Er), thulium (Tm), ytterbium (Yb) and lutetium (Lu)."@en ;
  rdfs:comment "UN Harmonized System codes 2805 (rare earths) and 8112 (rare earths)"@en ;
  :synonym "lanthanide"@en, "lanthanoid"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Lanthanum :Cerium :Praseodymium :Neodymium :Promethium :Samarium :Europium :Gadolinium :Terbium 
                                    :Dysorisium :Holmium :Erbium :Thulium :Ytterbium :Lutetium ) ] .

:ManganeseGroup a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 7) consisting of manganese (Mn), technetium (Tc), rhenium (Re) and bohrium (Bh)."@en ;
  rdfs:comment "UN Harmonized System codes 2844 (bohrium) and 8111 (manganese)"@en ;
  :synonym "Group 7 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Manganese :Technetium :Rhenium :Bohrium ) ] .

:NickelGroup a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 10) consisting of nickel (Ni), palladium (Pd), platium (Pt) and darmstadtium (Ds)."@en ;
  rdfs:comment "UN Harmonized System codes 2844 (darmstadtium) and 75 (nickel)"@en ;
  :synonym "Group 10 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Nickel :Palladium :Platinum :Darmstadtium ) ] .

:NobleGas a owl:Class ;
  rdfs:subClassOf :Chemical_InorganicAndOrganic ;
  :definition "A group in the periodic table (group 18) also known as an 'inert gas', 'rare gas' or 'aerogen'. It consists of the elements, helium (He), neon (Ne), argon (Ar), krypton (Kr), xenon (Xe), radon (Rn) and (potentially) oganesson (Og)."@en ;
  rdfs:comment "UN Harmonized System codes 2804 and 2844 (radon)"@en ;
  :synonym "inert gas"@en, "rare gas"@en, "aerogen"@en, "helium group"@en, "neon group"@en, "Group 18 (IUPAC)"@en ;
  owl:equivalentClass [ a owl:Class ;
                        owl:oneOf ( :Helium :Neon :Argon :Krypton :Xenon :Radon :Oganesson ) ] .

