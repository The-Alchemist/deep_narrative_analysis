  rdfs:subClassOf :Rubiaceae ;
  :synonym "quinine tree"@en, "red bark"@en, "cinchona"@en, "quina"@en, "quinquina"@en, "quinine bark"@en, "peruvian bark"@en, "jesuit's bark"@en .
