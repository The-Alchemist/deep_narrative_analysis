@prefix : <urn:ontoinsights:ontology:dna:> .
@prefix dna: <urn:ontoinsights:ontology:dna:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

########################################################################
# File defining semantics related to beverages
# The classes also reference the entities in codes 09 and 22 of the UN 
#    Commodities list (https://comtrade.un.org/db/mr/rfCommoditiesList.aspx)
# 
# Created: February 20, 2020
# Last modified: February 21, 2020
# 
# Changes:
#  * Added CarbonatedBeverage, not in the UN Commodities list (Feb 21 20)
########################################################################


########################################################################
# Classes
########################################################################

:Beer a owl:Class ;
  rdfs:subClassOf :Beverage ;
  :definition "Beer made from malt."@en ;
  rdfs:comment "UN Harmonized System code 2203"@en ;
  :synonym "ale"@en , "brew"@en, "suds"@en ;
  :example "lager"@en, "stout"@en, "pilsner"@en, "porter"@en, "bock"@en, "pilsner"@en .

:CarbonatedBeverage a owl:Class ;
  rdfs:subClassOf :Beverage ;
  :definition "Drink made with carbonated water."@en ;
  :synonym "soft drink"@en, "fizzy drink"@en, "carbonated drink"@en ;
  :example "soda"@en, "pop"@en, "soda pop"@en, "seltzer"@en, "tonic water"@en, "ginger ale"@en, "cola"@en, "energy drink"@en, "sports drink"@en, "diet drink"@en, "ginger beer"@en, "root beer"@en, "cream soda"@en .

:CiderAndMead a owl:Class ;
  rdfs:subClassOf :Beverage ;
  :definition "Fermented beverage made from fruit or honey."@en ;
  rdfs:comment "UN Harmonized System code 2206"@en ;
  :synonym "cider"@en, "perry"@en, "mead"@en, "honey-wine"@en, "honey wine"@en .

:CoffeeAndSubstitute a owl:Class ;
  rdfs:subClassOf :Beverage ;
  :definition "Beverage made by brewing beans or steeping leaves."@en ;
  rdfs:comment "UN Harmonized System codes 0901"@en ;
  :synonym "coffee"@en, "demitasse"@en, "java"@en, "cup of joe"@en ;
  :example "cappucino"@en, "expresso"@en, "cafe au lait"@en, "latte"@en .

:Liquor a owl:Class ;
  rdfs:subClassOf :Beverage ;
  :definition "Ethyl alcohol, spirits, liqueurs and other spirituous beverages."@en ;
  rdfs:comment "UN Harmonized System codes 2207 and 2208"@en ;
  :synonym "ethyl alcohol"@en, "booze"@en, "drink"@en, "liquer"@en, "spirit"@en, "cocktail"@en, "firewater"@en, "hootch"@en, "rotgut"@en, "moonshine"@en, "toddy"@en, "hard stuff"@en .

:Mate a owl:Class ;
  rdfs:subClassOf :Beverage ;
  :definition "Beverage made by steeping yerba mate leaves."@en ;
  rdfs:comment "UN Harmonized System codes 0903"@en ;
  :synonym "chimarrao"@en, "cimarron"@en .

:TeaAndSubstitute a owl:Class ;
  rdfs:subClassOf :Beverage ;
  :definition "Beverage made by steeping leaves."@en ;
  rdfs:comment "UN Harmonized System codes 0902"@en ;
  :synonym "tea"@en .

:Water a owl:Class ;
  rdfs:subClassOf :Beverage ;
  :definition "From Wikipedia (https://en.wikipedia.org/w/index.php?title=Water&oldid=932631915), Water is 'an inorganic, transparent, tasteless, odorless, and nearly colorless chemical substance, which is the main constituent of Earth's hydrosphere, and the fluids of most living organisms'. This includes natural or artificial mineral waters and aerated waters; ice and snow."@en ;
  rdfs:comment "UN Harmonized System codes 2201 and 2202"@en ;
  :synonym "H2O"@en .

:WineAndVermouth a owl:Class ;
  rdfs:subClassOf :Beverage ;
  :definition "An alcoholic drink make from fermented grapes."@en ;
  rdfs:comment "UN Harmonized System codes 2204 and 2205"@en ;
  :example "red blend"@en, "red wine"@en, "merlot"@en, "cabernet"@en, "bourdeaux"@en, "pinot noir"@fr, "syrah"@en, "shiraz"@en, "zinfandel"@en, "white wine"@en, "white blend"@en, "pinot grigio"@en, "pinot gris"@en, "chardonnay"@en, "sauvignon blanc"@en, "riesling"@en, "chenin blanc"@en, "gewurztraminer"@de, "grenache"@en, "rose (wine)"@en, "fruit wine"@en .


