########################################################################
# Classes
########################################################################

:H_AIRS a owl:Class ; 
  rdfs:subClassOf :WaterFeature, :AreaFeature ; 
  rdfs:label "seaplane landing area"@en ; 
  :definition "A place on a waterbody where floatplanes land and take off"@en . 

:H_ANCH a owl:Class ; 
  rdfs:subClassOf :WaterFeature, :AreaFeature ; 
  rdfs:label "anchorage"@en ; 
  :definition "An area where vessels may anchor"@en ;
  :synonym "berthage"@en .

:H_BAY a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "bay"@en ; 
  :definition "A coastal indentation between two capes or headlands, including coves and bights, but smaller than a gulf"@en ;
  :synonym "coastal indentation"@en, "coastal recession"@en, "cove"@en, "bight"@en .

:H_BNK a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "bank"@en ; 
  :definition "An elevation, typically located on a shelf, over which the depth of water is relatively shallow but sufficient for most surface navigation"@en . 

:H_BNKR a owl:Class ; 
  rdfs:subClassOf :H_BNK ; 
  rdfs:label "stream bank"@en ; 
  :definition "A sloping margin of a stream channel which normally confines the stream to its channel on land"@en ;
  :synonym "riparian"@en, "bankside"@en .

:H_BNKX a owl:Class ; 
  rdfs:subClassOf :H_BNK ; 
  rdfs:label "section of bank"@en ; 
  :definition "A section of a bank"@en . 

:H_BOG a owl:Class ; 
  rdfs:subClassOf :H_WTLD ; 
  rdfs:label "bog"@en ; 
  :definition "A wetland characterized by peat forming sphagnum moss, sedge, and other acid-water plants"@en ;
  :synonym "moorland"@en, "heath"@en, "quagmire"@en, "mire"@en, "moor"@en .

:H_CAPG a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "icecap"@en ; 
  :definition "A dome-shaped mass of glacial ice covering an area of mountain summits or other high lands; smaller than an ice sheet"@en .

:H_CHN a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "channel"@en ; 
  :definition "The deepest part of a stream, bay, lagoon, or strait, through which the main current flows"@en . 

:H_CHNL a owl:Class ; 
  rdfs:subClassOf :H_CHN ; 
  rdfs:label "lake channel"@en ; 
  :definition "That part of a lake having water deep enough for navigation between islands, shoals, etc."@en . 

:H_CHNM a owl:Class ; 
  rdfs:subClassOf :H_CHN ; 
  rdfs:label "marine channel"@en ; 
  :definition "That part of a body of water deep enough for navigation through an area otherwise not suitable"@en . 

:H_CHNN a owl:Class ; 
  rdfs:subClassOf :H_CHN, :TravelTransportRouteAndInfrastructure ; 
  rdfs:label "navigation channel"@en ; 
  :definition "A buoyed channel of sufficient depth for the safe navigation of vessels"@en . 

:H_CNFL a owl:Class ; 
  rdfs:subClassOf :WaterFeature, :Junction ;
  rdfs:label "confluence"@en ; 
  :definition "A place where two or more streams or intermittent streams flow together"@en ;
  :synonym "conflux"@en .

:H_CNL a owl:Class ; 
  rdfs:subClassOf :H_WTRC, :AnthropogenicFeature ; 
  rdfs:label "canal"@en ; 
  :definition "An artificial watercourse"@en . 

:H_CNLB a owl:Class ; 
  rdfs:subClassOf :H_CNLX ; 
  rdfs:label "canal bend"@en ; 
  :definition "A conspicuously curved or bent section of a canal"@en . 

:H_CNLD a owl:Class ; 
  rdfs:subClassOf :H_CNL ; 
  rdfs:label "drainage canal"@en ; 
  :definition "An artificial waterway carrying water away from a wetland or from drainage ditches"@en . 

:H_CNLI a owl:Class ; 
  rdfs:subClassOf :H_CNL ; 
  rdfs:label "irrigation canal"@en ; 
  :definition "A canal which serves as a main conduit for irrigation water"@en ;
  :synonym "acequia"@en, "aqueduct"@en .

:H_CNLN a owl:Class ; 
  rdfs:subClassOf :H_CNL, :TravelTransportRouteAndInfrastructure ; 
  rdfs:label "navigation canal"@en ; 
  :definition "A watercourse constructed for navigation of vessels"@en . 

:H_CNLQ a owl:Class ; 
  rdfs:subClassOf :H_CNL ; 
  rdfs:label "abandoned canal"@en ; 
  :definition "An abandoned canal"@en . 

:H_CNLSB a owl:Class ; 
  rdfs:subClassOf :H_CNLI ; 
  rdfs:label "underground irrigation canal"@en ; 
  :definition "A gently inclined underground tunnel bringing water for irrigation from aquifers"@en ;
  :synonym "qanat"@en, "subirrigation"@en .

:H_CNLX a owl:Class ; 
  rdfs:subClassOf :H_CNL ; 
  rdfs:label "section of canal"@en ; 
  :definition "A section of a canal"@en . 

:H_CRKT a owl:Class ; 
  rdfs:subClassOf :H_CHN ; 
  rdfs:label "tidal creek"@en ; 
  :definition "A meandering channel in a coastal wetland subject to bi-directional tidal currents"@en . 

:H_CRNT a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "current"@en ; 
  :definition "A horizontal flow of water in a given direction with uniform velocity"@en . 

:H_CUTF a owl:Class ; 
  rdfs:subClassOf :H_CHN ; 
  rdfs:label "cutoff"@en ; 
  :definition "A channel formed as a result of a stream cutting through a meander neck"@en . 

:H_DCK a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "dock"@en ; 
  :definition "A waterway between two piers, cut into the land, or other water location for the berthing of ships"@en . 

:H_DCKB a owl:Class ; 
  rdfs:subClassOf :H_DCK ; 
  rdfs:label "docking basin"@en ; 
  :definition "A part of a harbor where ships dock"@en . 

:H_DOMG a owl:Class ; 
  rdfs:subClassOf :H_CAPG ; 
  rdfs:label "icecap dome"@en ; 
  :definition "A comparatively elevated area on an icecap"@en . 

:H_DPRG a owl:Class ; 
  rdfs:subClassOf :H_CAPG ; 
  rdfs:label "icecap depression"@en ; 
  :definition "A comparatively depressed area on an icecap"@en . 

:H_DTCH a owl:Class ; 
  rdfs:subClassOf :H_CNL, :AnthropogenicFeature ; 
  rdfs:label "ditch"@en ; 
  :definition "A small artificial watercourse dug for draining or irrigating the land"@en ;
  :synonym "trench"@en, "moat"@en, "culvert"@en .

:H_DTCHM a owl:Class ; 
  rdfs:subClassOf :H_DTCH ; 
  rdfs:label "ditch mouth"@en ; 
  :definition "An area where a drainage ditch enters a lagoon, lake or bay"@en . 

:H_ESTY a owl:Class ; 
  rdfs:subClassOf :H_INLT ; 
  rdfs:label "estuary"@en ; 
  :definition "A funnel-shaped stream mouth or embayment where fresh water mixes with sea water under tidal influences"@en ;
  :synonym "tidewater"@en, "embayment"@en, "firth"@en .

:H_FISH a owl:Class ; 
  rdfs:subClassOf :WaterFeature, :AreaFeature ; 
  rdfs:label "fishing area"@en ; 
  :definition "A fishing ground, bank or area where fishermen go to catch fish"@en . 

:H_FJD a owl:Class ; 
  rdfs:subClassOf :H_INLT ; 
  rdfs:label "fjord"@en ; 
  :definition "A long, narrow, steep-walled, deep-water arm of the sea at high latitudes, usually along mountainous coasts"@en . 

:H_FLLS a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "waterfall"@en ; 
  :definition "A perpendicular or very steep descent of the water of a stream"@en ;
  :synonym "cataract"@en, "falls"@en, "cascades"@en .

:H_FLLSX a owl:Class ; 
  rdfs:subClassOf :H_FLLS ; 
  rdfs:label "section of waterfall"@en ; 
  :definition "A section of a waterfall"@en . 

:H_FLTM a owl:Class ; 
  rdfs:subClassOf :H_WTLD ; 
  rdfs:label "mud flat"@en ; 
  :definition "A relatively level area of mud either between high and low tide lines, or subject to flooding"@en ;
  :synonym "mudflat"@en .

:H_FLTT a owl:Class ; 
  rdfs:subClassOf :H_WTLD ; 
  rdfs:label "tidal flat"@en ; 
  :definition "A large flat area of mud or sand attached to the shore and alternately covered and uncovered by the tide"@en ;
  :synonym "intertidal"@en .

:H_GLCR a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "glacier"@en ; 
  :definition "A mass of ice, usually at high latitudes or high elevations, with sufficient thickness to flow away from the source area in lobes, tongues, or masses"@en ;
  :synonym "icefield"@en .

:H_GULF a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "gulf"@en ; 
  :definition "A large recess in the coastline, larger than a bay"@en . 

:H_GYSR a owl:Class ; 
  rdfs:subClassOf :H_SPNT ; 
  rdfs:label "geyser"@en ; 
  :definition "A type of hot spring with intermittent eruptions of jets of hot water and steam"@en . 

:H_HBR a owl:Class ; 
  rdfs:subClassOf :H_ANCH, :AreaFeature ; 
  rdfs:label "harbor"@en ; 
  :definition "A haven or space of deep water so sheltered by the adjacent land as to afford a safe anchorage for ships"@en ;
  :synonym "seaport"@en, "harbour"@en .

:H_HBRX a owl:Class ; 
  rdfs:subClassOf :H_HBR ; 
  rdfs:label "section of harbor"@en ; 
  :definition "A section of a harbor"@en . 

:H_INLT a owl:Class ; 
  rdfs:subClassOf :H_WTRC ; 
  rdfs:label "inlet"@en ; 
  :definition "A narrow waterway extending into the land, or connecting a bay or lagoon with a larger body of water"@en ;
  :synonym "estero"@en .

:H_INLTQ a owl:Class ; 
  rdfs:subClassOf :H_INLT ; 
  rdfs:label "former inlet"@en ; 
  :definition "An inlet which has been filled in, or blocked by deposits"@en . 

:H_LBED a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "lake bed"@en ; 
  :definition "A dried up or drained area of a former lake"@en . 

:H_LGN a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "lagoon"@en ; 
  :definition "A shallow coastal waterbody, completely or partly separated from a larger body of water by a barrier island, coral reef or other depositional feature"@en ;
  :synonym "laguna"@en, "lagune"@en .

:H_LGNX a owl:Class ; 
  rdfs:subClassOf :H_LGN ; 
  rdfs:label "section of lagoon"@en ; 
  :definition "A section of a lagoon"@en . 

:H_LK a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "lake"@en ; 
  :definition "A large inland body of standing water"@en ;
  :synonym "loch"@en, "maar"@en .

:H_LKI a owl:Class ; 
  rdfs:subClassOf :H_LK ; 
  rdfs:label "intermittent lake"@en ; 
  :definition "An intermittent lake"@en . 

:H_LKN a owl:Class ; 
  rdfs:subClassOf :H_LK ; 
  rdfs:label "salt lake"@en ; 
  :definition "An inland body of salt water with no outlet"@en ;
  :synonym "saline lake"@en .

:H_LKO a owl:Class ; 
  rdfs:subClassOf :H_LK ; 
  rdfs:label "oxbow lake"@en ; 
  :definition "A crescent-shaped lake commonly found adjacent to meandering streams"@en . 

:H_LKOI a owl:Class ; 
  rdfs:subClassOf :H_LKO, :H_LKI ; 
  rdfs:label "intermittent oxbow lake"@en ; 
  :definition "An intermittent oxbow lake"@en . 

:H_LKSB a owl:Class ; 
  rdfs:subClassOf :H_LK ; 
  rdfs:label "underground lake"@en ; 
  :definition "A standing body of water in a cave"@en . 

:H_LKX a owl:Class ; 
  rdfs:subClassOf :H_LK ; 
  rdfs:label "section of lake"@en ; 
  :definition "A section of a lake"@en . 

:H_MFGN a owl:Class ; 
  rdfs:subClassOf :WaterFeature, :L_SALT ; 
  rdfs:label "salt evaporation pond"@en ; 
  :definition "A diked salt pond used in the production of solar evaporated salt"@en . 

:H_MGV a owl:Class ; 
  rdfs:subClassOf :H_SWMP ; 
  rdfs:label "mangrove swamp"@en ; 
  :definition "A tropical tidal mud flat characterized by mangrove vegetation"@en ;
  :synonym "mangal"@en, "mangrove island"@en .

:H_MRSH a owl:Class ; 
  rdfs:subClassOf :H_WTLD ; 
  rdfs:label "marsh"@en ; 
  :definition "A wetland dominated by grass-like vegetation"@en ;
  :synonym "marshland"@en, "fen"@en, "bayou"@en, "swale"@en .

:H_MRSHN a owl:Class ; 
  rdfs:subClassOf :H_MRSH, :L_SALT ; 
  rdfs:label "salt marsh"@en ; 
  :definition "A flat area, subject to periodic salt water inundation, dominated by grassy salt-tolerant plants"@en . 

:H_NRWS a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "narrows"@en ; 
  :definition "A navigable narrow part of a bay, strait, river, etc."@en . 

:H_OCN a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "ocean"@en ; 
  :definition "One of the major divisions of the vast expanse of salt water covering part of the earth"@en ;
  :synonym "high seas"@en .

:H_OVF a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "overfalls"@en ; 
  :definition "An area of breaking waves caused by the meeting of currents or by waves moving against the current"@en ;
  :synonym "breaking waves"@en .

:H_PND a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "pond"@en ; 
  :definition "A small standing waterbody"@en ;
  :synonym "billabong"@en .

:H_PNDI a owl:Class ; 
  rdfs:subClassOf :H_PND ; 
  rdfs:label "intermittent pond"@en ; 
  :definition "An intermittent pond"@en . 

:H_PNDN a owl:Class ; 
  rdfs:subClassOf :H_PND ; 
  rdfs:label "salt pond"@en ; 
  :definition "A small standing body of salt water often in a marsh or swamp, usually along a seacoast"@en . 

:H_PNDSF a owl:Class ; 
  rdfs:subClassOf :H_PND, :AreaFeature ; 
  rdfs:label "fishpond"@en ; 
  :definition "A pond or enclosure in which fish are kept or raised"@en . 

:H_POOL a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "pool"@en ; 
  :definition "A small and comparatively still, deep part of a larger body of water such as a stream or harbor; or a small body of standing water"@en . 

:H_POOLI a owl:Class ; 
  rdfs:subClassOf :H_POOL ; 
  rdfs:label "intermittent pool"@en ; 
  :definition "An intermittent pool"@en . 

:H_RCH a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "reach"@en ; 
  :definition "A straight section of a navigable stream or channel between two bends"@en . 

:H_RDGG a owl:Class ; 
  rdfs:subClassOf :H_CAPG ; 
  rdfs:label "icecap ridge"@en ; 
  :definition "A linear elevation on an icecap"@en ;
  :synonym "ice hummock"@en .

:H_RDST a owl:Class ; 
  rdfs:subClassOf :H_ANCH ; 
  rdfs:label "roadstead"@en ; 
  :definition "An open anchorage affording less protection than a harbor"@en . 

:H_RF a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "reef"@en ; 
  :definition "A surface-navigation hazard composed of consolidated material"@en . 

:H_RFC a owl:Class ; 
  rdfs:subClassOf :H_RF ; 
  rdfs:label "coral reef"@en ; 
  :definition "A surface-navigation hazard composed of coral"@en ;
  :synonym "barrier reef"@en .

:H_RFX a owl:Class ; 
  rdfs:subClassOf :H_RF ; 
  rdfs:label "section of reef"@en ; 
  :definition "A section of a reef"@en . 

:H_RPDS a owl:Class ; 
  rdfs:subClassOf :H_STM ; 
  rdfs:label "rapids"@en ; 
  :definition "A turbulent section of a stream associated with a steep, irregular stream bed"@en ;
  :synonym "whitewater"@en .

:H_RSV a owl:Class ; 
  rdfs:subClassOf :WaterFeature, :AnthropogenicFeature ; 
  rdfs:label "reservoir"@en ; 
  :definition "An artificial pond or lake"@en . 

:H_RSVI a owl:Class ; 
  rdfs:subClassOf :H_RSV ; 
  rdfs:label "intermittent reservoir"@en ; 
  :definition "An intermittent reservoir"@en . 

:H_RSVT a owl:Class ; 
  rdfs:subClassOf :H_RSV  ; 
  rdfs:label "water tank"@en ; 
  :definition "A contained pool or tank of water at, below, or above ground level"@en ;
  :synonym "cistern"@en, "water butt"@en .

:H_RVN a owl:Class ; 
  rdfs:subClassOf :T_VAL ; 
  rdfs:label "ravine"@en ; 
  :definition "A small, narrow, deep, steep-sided stream channel, smaller than a gorge"@en ;
  :synonym "nullah"@en, "gulch"@en, "arroyo"@en, "gully"@en .

:H_SBKH a owl:Class ; 
  rdfs:subClassOf :WaterFeature, :L_SALT, :T_PLN ; 
  rdfs:label "sabkha"@en ; 
  :definition "A salt flat or salt encrusted plain subject to periodic inundation from flooding or high tides"@en . 

:H_SD a owl:Class ; 
  rdfs:subClassOf :H_WTRC ; 
  rdfs:label "sound"@en ; 
  :definition "A long arm of the sea forming a channel between the mainland and an island or islands; or connecting two larger bodies of water"@en . 

:H_SEA a owl:Class ; 
  rdfs:subClassOf :H_OCN ; 
  rdfs:label "sea"@en ; 
  :definition "A large body of salt water more or less confined by continuous land or chains of islands forming a subdivision of an ocean"@en . 

:H_SHOL a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "shoal"@en ; 
  :definition "A surface-navigation hazard composed of unconsolidated material"@en ;
  :synonym "sandbar"@en, "sandbank"@en .

:H_SILL a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "sill"@en ; 
  :definition "The low part of an underwater gap or saddle separating basins, including a similar feature at the mouth of a fjord"@en . 

:H_SPNG a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "spring"@en ; 
  :definition "A place where ground water flows naturally out of the ground"@en . 

:H_SPNS a owl:Class ; 
  rdfs:subClassOf :H_SPNG ; 
  rdfs:label "sulphur spring"@en ; 
  :definition "A place where sulphur ground water flows naturally out of the ground"@en . 

:H_SPNT a owl:Class ; 
  rdfs:subClassOf :H_SPNG ; 
  rdfs:label "hot spring"@en ; 
  :definition "A place where hot ground water flows naturally out of the ground"@en ;
  :synonym "thermal spring"@en .

:H_STM a owl:Class ; 
  rdfs:subClassOf :H_WTRC ; 
  rdfs:label "stream"@en ; 
  :definition "A body of running water moving to a lower level in a channel on land"@en ;
  :synonym "river"@en, "tributary"@en, "creek"@en, "brook"@en, "coulee"@en, "runlet"@en .

:H_STMA a owl:Class ; 
  rdfs:subClassOf :H_STM ; 
  rdfs:label "anabranch"@en ; 
  :definition "A diverging branch flowing out of a main stream and rejoining it downstream"@en ;
  :synonym "corrivation"@en .

:H_STMB a owl:Class ; 
  rdfs:subClassOf :H_STM ; 
  rdfs:label "stream bend"@en ; 
  :definition "A conspicuously curved or bent segment of a stream"@en . 

:H_STMC a owl:Class ; 
  rdfs:subClassOf :H_STM, :H_CNL ; 
  rdfs:label "canalized stream"@en ; 
  :definition "A stream that has been substantially ditched, diked, or straightened"@en . 

:H_STMD a owl:Class ; 
  rdfs:subClassOf :H_STM ; 
  rdfs:label "distributary"@en ; 
  :definition "A branch which flows away from the main stream, as in a delta or irrigation canal"@en . 

:H_STMH a owl:Class ; 
  rdfs:subClassOf :H_STM ; 
  rdfs:label "headwater"@en ; 
  :definition "The source and upper part of a stream, including the upper drainage basin"@en . 

:H_STMI a owl:Class ; 
  rdfs:subClassOf :H_STM ; 
  rdfs:label "intermittent stream"@en ; 
  :definition "An intermittent stream"@en ;
  :synonym "dry wash"@en, "bourn"@en .

:H_STMIX a owl:Class ; 
  rdfs:subClassOf :H_STMI ; 
  rdfs:label "section of intermittent stream"@en ; 
  :definition "A section of an intermittent stream"@en . 

:H_STMM a owl:Class ; 
  rdfs:subClassOf :H_STM ; 
  rdfs:label "stream mouth"@en ; 
  :definition "A place where a stream discharges into a lagoon, lake, or the sea"@en . 

:H_STMQ a owl:Class ; 
  rdfs:subClassOf :H_WTRC ; 
  rdfs:label "abandoned watercourse"@en ; 
  :definition "A former stream or distributary no longer carrying flowing water, but still evident due to lakes, wetland, topographic or vegetation patterns"@en . 

:H_STMSB a owl:Class ; 
  rdfs:subClassOf :H_STM ; 
  rdfs:label "lost river"@en ; 
  :definition "A surface stream that disappears into an underground channel, or dries up in an arid area"@en . 

:H_STMX a owl:Class ; 
  rdfs:subClassOf :H_STM ; 
  rdfs:label "section of stream"@en ; 
  :definition "A section of a stream"@en . 

:H_STRT a owl:Class ; 
  rdfs:subClassOf :H_WTRC ; 
  rdfs:label "strait"@en ; 
  :definition "A relatively narrow waterway, usually narrower and less extensive than a sound, connecting two larger bodies of water"@en . 

:H_SWMP a owl:Class ; 
  rdfs:subClassOf :H_WTLD ; 
  rdfs:label "swamp"@en ; 
  :definition "A wetland dominated by tree vegetation"@en ;
  :synonym "slue"@en .

:H_SYSI a owl:Class ; 
  rdfs:subClassOf :WaterFeature, :AnthropogenicFeature ; 
  rdfs:label "irrigation system"@en ; 
  :definition "A network of ditches and one or more of the following elements: water supply, reservoir, canal, pump, well, drain, etc."@en . 

:H_TNLC a owl:Class ; 
  rdfs:subClassOf :H_CNL ; 
  rdfs:label "canal tunnel"@en ; 
  :definition "A tunnel through which a canal passes"@en . 

:H_WAD a owl:Class ; 
  rdfs:subClassOf :H_WTRC, :T_VAL ; 
  rdfs:label "wadi"@en ; 
  :definition "A valley or ravine, bounded by relatively steep banks, which in the rainy season becomes a watercourse; found primarily in North Africa and the Middle East"@en . 

:H_WADB a owl:Class ; 
  rdfs:subClassOf :H_WAD ; 
  rdfs:label "wadi bend"@en ; 
  :definition "A conspicuously curved or bent segment of a wadi"@en . 

:H_WADJ a owl:Class ; 
  rdfs:subClassOf :H_WAD, :Junction ; 
  rdfs:label "wadi junction"@en ; 
  :definition "A place where two or more wadies join"@en . 

:H_WADM a owl:Class ; 
  rdfs:subClassOf :H_WAD ; 
  rdfs:label "wadi mouth"@en ; 
  :definition "The lower terminus of a wadi where it widens into an adjoining floodplain, depression, or waterbody"@en . 

:H_WADX a owl:Class ; 
  rdfs:subClassOf :H_WAD ; 
  rdfs:label "section of wadi"@en ; 
