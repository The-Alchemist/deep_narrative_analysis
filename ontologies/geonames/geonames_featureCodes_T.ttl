@prefix : <urn:ontoinsights:ontology:dna:> .
@prefix dna: <urn:ontoinsights:ontology:dna:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

########################################################################
# File defining semantics of hypsographic/topological features
#    (GeoNames' 'T' feature codes)
# 
# Created: February 14, 2020
# Last modified: February 14, 2020
########################################################################


########################################################################
# Classes
########################################################################

:Elevation a owl:Class ;
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "elevation"@en ; 
  :definition "A raised (above sea level) geographic formation"@en ;
  :synonym "rise"@en ;
  :antonym :T_DPR .
 
:T_ASPH a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "asphalt lake"@en ; 
  :definition "A small basin containing naturally occurring asphalt"@en . 

:T_ATOL a owl:Class ; 
  rdfs:subClassOf :T_ISL, :H_RFC ; 
  rdfs:label "atoll"@en ; 
  :definition "A ring-shaped coral reef which has closely spaced islands on it encircling a lagoon"@en . 

:T_BAR a owl:Class ; 
  rdfs:subClassOf :T_RDGE ; 
  rdfs:label "point bar"@en ; 
  :definition "A shallow ridge or mound of coarse unconsolidated material in a stream channel, at the mouth of a stream, estuary, or lagoon and in the wave-break zone along coasts"@en . 

:T_BDLD a owl:Class ; 
  rdfs:subClassOf :T_KRST ; 
  rdfs:label "badlands"@en ; 
  :definition "An area characterized by a maze of very closely spaced, deep, narrow, steep-sided ravines, and sharp crests and pinnacles"@en . 

:T_BLDR a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "boulder field"@en ; 
  :definition "A high altitude or high latitude bare, flat area covered with large angular rocks"@en . 

:T_BLHL a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature, :WaterFeature ; 
  rdfs:label "blowhole"@en ; 
  :definition "A hole in coastal rock through which sea water is forced by a rising tide or waves and spurted through an outlet into the air"@en . 

:T_BLOW a owl:Class ; 
  rdfs:subClassOf :T_DPR ; 
  rdfs:label "blowout"@en ; 
  :definition "A small depression in sandy terrain, caused by wind erosion"@en . 

:T_BUTE a owl:Class ; 
  rdfs:subClassOf :T_HLL ; 
  rdfs:label "butte"@en ; 
  :definition "A small, isolated, usually flat-topped hill with steep sides"@en . 

:T_CAPE a owl:Class ; 
  rdfs:subClassOf :T_HDLD ; 
  rdfs:label "cape"@en ; 
  :definition "A land area, more prominent than a point, projecting into the sea and marking a notable change in coastal direction"@en . 

:T_CFT a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "cleft"@en ; 
  :definition "A deep narrow slot, notch, or groove in a coastal cliff"@en . 

:T_CLDA a owl:Class ; 
  rdfs:subClassOf :T_DPR ; 
  rdfs:label "caldera"@en ; 
  :definition "A depression measuring kilometers across formed by the collapse of a volcanic mountain"@en . 

:T_CLF a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "cliff"@en ; 
  :definition "A high, steep to perpendicular slope overlooking a waterbody or lower area"@en ;
  :synonym "precipice"@en . 

:T_CNYN a owl:Class ; 
  rdfs:subClassOf :T_VAL ; 
  rdfs:label "canyon"@en ; 
  :definition "A deep, narrow valley with steep sides cutting into a plateau or mountainous area"@en . 

:T_CONE a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "cone"@en ; 
  :definition "A conical landform composed of mud or volcanic material"@en . 

:T_CRQ a owl:Class ; 
  rdfs:subClassOf :T_VAL ; 
  rdfs:label "cirque"@en ; 
  :definition "A bowl-like hollow partially surrounded by cliffs or steep slopes at the head of a glaciated valley"@en ;
  :synonym "cwm"@en, "corrie"@en .

:T_CRTR a owl:Class ; 
  rdfs:subClassOf :T_DPR ; 
  rdfs:label "crater"@en ; 
  :definition "A generally circular saucer or bowl-shaped depression caused by volcanic or meteorite explosive action"@en . 

:T_CUET a owl:Class ; 
  rdfs:subClassOf :T_RDGE ; 
  rdfs:label "cuesta"@en ; 
  :definition "An asymmetric ridge formed on tilted strata"@en . 

:T_DPR a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "depression"@en ; 
  :definition "A low area surrounded by higher land and usually characterized by interior drainage"@en . 

:T_DSRT a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "desert"@en ; 
  :definition "A large area with little or no vegetation due to extreme environmental conditions"@en ;
  :synonym "arid region"@en .

:T_DUNE a owl:Class ; 
  rdfs:subClassOf :T_SAND ; 
  rdfs:label "dune"@en ; 
  :definition "A wave form, ridge or star shape feature composed of sand"@en . 

:T_DVD a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "drainage divide"@en ; 
  :definition "A line separating adjacent drainage basins"@en . 

:T_ERG a owl:Class ; 
  rdfs:subClassOf :T_DSRT, :T_DUNE ; 
  rdfs:label "sandy desert"@en ; 
  :definition "An extensive tract of shifting sand and sand dunes"@en . 

:T_FAN a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "alluvial fan"@en ; 
  :definition "A fan-shaped wedge of coarse alluvium with apex merging with a mountain stream bed and the fan spreading out at a low angle slope onto an adjacent plain"@en ;
  :synonym "delta"@en .

:T_FORD a owl:Class ; 
  rdfs:subClassOf :TravelTransportRouteAndInfrastructure, :HypsographicFeature, :H_STM ; 
  rdfs:label "ford"@en ; 
  :definition "A shallow part of a stream which can be crossed on foot or by land vehicle"@en . 

:T_FSR a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "fissure"@en ; 
  :definition "A crack associated with volcanism"@en . 

:T_GAP a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "gap"@en ; 
  :definition "A low place in a ridge or high obstruction"@en . 

:T_GRGE a owl:Class ; 
  rdfs:subClassOf :T_VAL ; 
  rdfs:label "gorge"@en ; 
  :definition "A short, narrow, steep-sided section of a stream valley"@en . 

:T_HDLD a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "headland"@en ; 
  :definition "A high projection of land extending into a large body of water beyond the line of the coast"@en ;
  :synonym "foreland"@en .

:T_HLL a owl:Class ; 
  rdfs:subClassOf :Elevation ; 
  rdfs:label "hill"@en ; 
  :definition "A rounded elevation of limited extent rising above the surrounding land"@en .

:T_HMCK a owl:Class ; 
  rdfs:subClassOf :T_MND ; 
  rdfs:label "hummock"@en ; 
  :definition "A patch of ground, distinct from and slightly above the surrounding plain or wetland (often occurs in groups)"@en . 

:T_HMDA a owl:Class ; 
  rdfs:subClassOf :T_DSRT ; 
  rdfs:label "rock desert"@en ; 
  :definition "A relatively sand-free, high bedrock plateau in a hot desert, with or without a gravel veneer"@en . 

:T_INTF a owl:Class ; 
  rdfs:subClassOf :T_UPLD ; 
  rdfs:label "interfluve"@en ; 
  :definition "A relatively undissected upland between adjacent stream valleys"@en . 

:T_ISL a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "island"@en ; 
  :definition "A tract of land, smaller than a continent, surrounded by water at high water"@en ;
  :synonym "archipelago"@en, "isle"@en, "islet"@en .

:T_ISLF a owl:Class ; 
  rdfs:subClassOf :T_ISL, :AnthropogenicFeature ; 
  rdfs:label "artificial island"@en ; 
  :definition "An island created by landfill or diking and filling in a wetland, bay, or lagoon"@en ;
  :synonym "crannog"@en .

:T_ISLT a owl:Class ; 
  rdfs:subClassOf :T_ISL ; 
  rdfs:label "land-tied island"@en ; 
  :definition "A coastal island connected to the mainland by barrier beaches, levees or dikes"@en . 

:T_ISLX a owl:Class ; 
  rdfs:subClassOf :T_ISL ; 
  rdfs:label "section of island"@en ; 
  :definition "A section of an island"@en . 

:T_ISTH a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "isthmus"@en ; 
  :definition "A narrow strip of land connecting two larger land masses and bordered by water"@en . 

:T_KRST a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "karst area"@en ; 
  :definition "A distinctive landscape developed on soluble rock such as limestone characterized by sinkholes, caves, disappearing streams, and underground drainage"@en ;
  :synonym "polje"@en, "uvala"@en .

:T_LAVA a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "lava area"@en ; 
  :definition "An area of solidified lava"@en ;
  :synonym "lava field"@en, "lava plain"@en, "lava bed"@en .

:T_MESA a owl:Class ; 
  rdfs:subClassOf :Elevation ; 
  rdfs:label "mesa"@en ; 
  :definition "A flat-topped, isolated elevation with steep slopes on all sides, less extensive than a plateau"@en . 

:T_MND a owl:Class ; 
  rdfs:subClassOf :T_HLL ; 
  rdfs:label "mound"@en ; 
  :definition "A low, isolated, rounded hill"@en ;
  :synonym "knoll"@en .

:T_MRN a owl:Class ; 
  rdfs:subClassOf :T_MND ; 
  rdfs:label "moraine"@en ; 
  :definition "A mound, ridge, or other accumulation of glacial till"@en . 

:T_MT a owl:Class ; 
  rdfs:subClassOf :Elevation ; 
  rdfs:label "mountain"@en ; 
  :definition "An elevation standing high above the surrounding area with small summit area, steep slopes and local relief of 300m or more"@en ;
  :synonym "highland"@en, "mount"@en, "mountainside"@en, "mountaintop"@en, "mountainous region"@en, "apline region"@en, "sub-alpine region"@en, "crag"@en, "cragg"@en, "craig"@en .

:T_NKM a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "meander neck"@en ; 
  :definition "A narrow strip of land between the two limbs of a meander loop at its narrowest point"@en . 

:T_NTK a owl:Class ; 
  rdfs:subClassOf :T_PK ; 
  rdfs:label "nunatak"@en ; 
  :definition "A rock or mountain peak protruding through glacial ice"@en ;
  :synonym "glacial island"@en .

:T_PAN a owl:Class ; 
  rdfs:subClassOf :T_DPR ; 
  rdfs:label "pan"@en ; 
  :definition "A near-level shallow, natural depression or basin, usually containing an intermittent lake, pond, or pool"@en . 

:T_PASS a owl:Class ; 
  rdfs:subClassOf :TravelTransportRouteAndInfrastructure, :T_GAP ; 
  rdfs:label "pass"@en ; 
  :definition "A break or low area in a mountain range or other high obstruction, used for transportation from one side to the other"@en . 

:T_PEN a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "peninsula"@en ; 
  :definition "An elongate area of land projecting into a body of water and nearly surrounded by water"@en . 

:T_PENX a owl:Class ; 
  rdfs:subClassOf :T_PEN ; 
  rdfs:label "section of peninsula"@en ; 
  :definition "A section of a peninsula"@en . 

:T_PK a owl:Class ; 
  rdfs:subClassOf :Elevation ; 
  rdfs:label "peak"@en ; 
  :definition "A pointed elevation atop a mountain, ridge, or other hypsographic feature"@en ;
  :synonym "summit"@en, "pinnacle"@en, "aiguille"@en . 

:T_PLAT a owl:Class ; 
  rdfs:subClassOf :T_PLN, :Elevation ; 
  rdfs:label "plateau"@en ; 
  :definition "An elevated plain with steep slopes on one or more sides, and often with incised streams"@en ;
  :synonym "tableland"@en .

:T_PLATX a owl:Class ; 
  rdfs:subClassOf :T_PLAT ; 
  rdfs:label "section of plateau"@en ; 
  :definition "A section of a plateau"@en . 

:T_PLDR a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "polder"@en ; 
  :definition "An area reclaimed from the sea by diking and draining"@en . 

:T_PLN a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "plain"@en ; 
  :definition "An extensive area of comparatively level to gently undulating land, lacking surface irregularities, and usually adjacent to a higher area"@en . 

:T_PLNX a owl:Class ; 
  rdfs:subClassOf :T_PLN ; 
  rdfs:label "plain"@en ; 
  :definition "A section of a plain"@en . 

:T_PROM a owl:Class ; 
  rdfs:subClassOf :T_HLL ; 
  rdfs:label "promontory"@en ; 
  :definition "A bluff or prominent hill overlooking or projecting into a lowland"@en . 

:T_PT a owl:Class ; 
  rdfs:subClassOf :T_HDLD ; 
  rdfs:label "point"@en ; 
  :definition "A tapering piece of land projecting into a body of water, less prominent than a cape"@en .

:T_RDGB a owl:Class ; 
  rdfs:subClassOf :T_RDGE, :T_SAND ; 
  rdfs:label "beach ridge"@en ; 
  :definition "A ridge of sand just inland and parallel to the beach, usually in series"@en . 

:T_RDGE a owl:Class ; 
  rdfs:subClassOf :Elevation ; 
  rdfs:label "ridge"@en ; 
  :definition "A long narrow elevation with steep sides, and a more or less continuous crest"@en . 

:T_REG a owl:Class ; 
  rdfs:subClassOf :T_DSRT, :T_PLN ; 
  rdfs:label "stony desert"@en ; 
  :definition "A desert plain characterized by a surface veneer of gravel and stones"@en . 

:T_RK a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "rock"@en ; 
  :definition "A conspicuous, isolated rocky mass"@en . 

:T_RKFL a owl:Class ; 
  rdfs:subClassOf :T_SLID ; 
  rdfs:label "rockfall"@en ; 
  :definition "An irregular mass of fallen rock at the base of a cliff or steep slope"@en ;
  :synonym "rockslide"@en .

:T_SAND a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "sand area"@en ; 
  :definition "A tract of land covered with sand"@en . 

:T_SBED a owl:Class ; 
  rdfs:subClassOf :WaterFeature ; 
  rdfs:label "dry stream bed"@en ; 
  :definition "A channel formerly containing the water of a stream"@en . 

:T_SCRP a owl:Class ; 
  rdfs:subClassOf :T_CLF ; 
  rdfs:label "escarpment"@en ; 
  :definition "A long line of cliffs or steep slopes separating level surfaces above and below"@en ;
  :synonym "scarp"@en .

:T_SDL a owl:Class ; 
  rdfs:subClassOf :T_PASS ; 
  rdfs:label "saddle"@en ; 
  :definition "A broad, open pass crossing a ridge or between hills or mountains"@en ;
  :synonym "saddleback"@en .

:T_SINK a owl:Class ; 
  rdfs:subClassOf :T_DPR, :T_KRST ; 
  rdfs:label "sinkhole"@en ; 
  :definition "A small crater-shape depression in a karst area"@en ;
  :synonym "swallow hole"@en . 

:T_SLID a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "slide"@en ; 
  :definition "A mound of earth material, at the base of a slope and the associated scoured area"@en ;
  :synonym "landslide"@en, "landslip"@en, "mudslide"@en .

:T_SLP a owl:Class ; 
  rdfs:subClassOf :Elevation ; 
  rdfs:label "slope"@en ; 
  :definition "A surface with a relatively uniform slope angle"@en . 

:T_SPIT a owl:Class ; 
  rdfs:subClassOf :L_CST ; 
  rdfs:label "spit"@en ; 
  :definition "A narrow, straight or curved continuation of a beach into a waterbody"@en . 

:T_SPUR a owl:Class ; 
  rdfs:subClassOf :T_RDGE ; 
  rdfs:label "spur"@en ; 
  :definition "A subordinate ridge projecting outward from a hill, mountain or other elevation"@en . 

:T_TAL a owl:Class ; 
  rdfs:subClassOf :T_SLP, :T_SLID ; 
  rdfs:label "talus slope"@en ; 
  :definition "A steep concave slope formed by an accumulation of loose rock fragments at the base of a cliff or steep slope"@en . 

:T_TRGD a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "interdune trough"@en ; 
  :definition "A long wind-swept trough between parallel longitudinal dunes"@en . 

:T_TRR a owl:Class ; 
  rdfs:subClassOf :HypsographicFeature ; 
  rdfs:label "terrace or bench"@en ; 
  :definition "A long, narrow platform bounded by steeper slopes above and below, usually overlooking a waterbody"@en . 

:T_UPLD a owl:Class ; 
  rdfs:subClassOf :Elevation ; 
  rdfs:label "upland"@en ; 
  :definition "An extensive interior region of high land with low to moderate surface relief"@en . 

:T_VAL a owl:Class ; 
  rdfs:subClassOf :T_DPR ; 
  rdfs:label "valley"@en ; 
  :definition "An elongated depression usually traversed by a stream"@en ;
  :synonym "vale"@en .

:T_VALG a owl:Class ; 
  rdfs:subClassOf :T_VAL ; 
  rdfs:label "hanging valley"@en ; 
  :definition "A valley the floor of which is notably higher than the valley or shore to which it leads; most common in areas that have been glaciated"@en . 

:T_VALX a owl:Class ; 
  rdfs:subClassOf :T_VAL ; 
  rdfs:label "section of valley"@en ; 
  :definition "A section of a valley"@en . 

:T_VLC a owl:Class ; 
  rdfs:subClassOf :Elevation ; 
  rdfs:label "volcano"@en ; 
  :definition "A conical elevation composed of volcanic materials with a crater at the top"@en . 
