########################################################################
# Classes
########################################################################

:V_BUSH a owl:Class ; 
  rdfs:subClassOf :VegetationFeature ; 
  rdfs:label "bush"@en ; 
  :definition "A small clump of conspicuous bushes in an otherwise bare area"@en . 

:V_CULT a owl:Class ; 
  rdfs:subClassOf :VegetationFeature ; 
  rdfs:label "cultivated area"@en ; 
  :definition "An area under cultivation"@en ;
  :synonym "shamba"@en .

:V_FRST a owl:Class ; 
  rdfs:subClassOf :VegetationFeature, :AreaFeature ; 
  rdfs:label "forest"@en ; 
  :definition "An area dominated by tree vegetation"@en ;
  :synonym "woodland"@en, "coppice"@en, "timberland"@en, "woods"@en .

:V_FRSTF a owl:Class ; 
  rdfs:subClassOf :V_FRST ; 
  rdfs:label "fossilized forest"@en ; 
  :definition "A forest fossilized by geologic processes and now exposed at the earth's surface"@en ;
  :synonym "petrified forest"@en .

:V_GROVE a owl:Class ; 
  rdfs:subClassOf :VegetationFeature ; 
  rdfs:label "grove"@en ; 
  :definition "A small wooded area or collection of trees growing closely together, occurring naturally or deliberately planted"@en ;
  :synonym "copse"@en, "stand"@en .

:V_GRSLD a owl:Class ; 
  rdfs:subClassOf :VegetationFeature ; 
  rdfs:label "grassland"@en ; 
  :definition "An area dominated by grass vegetation"@en ;
  :synonym "steppe"@en, "lea"@en, "savanna"@en, "prairie"@en .

:V_GRVC a owl:Class ; 
  rdfs:subClassOf :V_GROVE ; 
  rdfs:label "coconut grove"@en ; 
  :definition "A planting of coconut trees"@en . 

:V_GRVO a owl:Class ; 
  rdfs:subClassOf :V_GROVE ; 
  rdfs:label "olive grove"@en ; 
  :definition "A planting of olive trees"@en . 

:V_GRVP a owl:Class ; 
  rdfs:subClassOf :V_GROVE ; 
  rdfs:label "palm grove"@en ; 
  :definition "A planting of palm trees"@en . 

:V_GRVPN a owl:Class ; 
  rdfs:subClassOf :V_GROVE ; 
  rdfs:label "pine grove"@en ; 
  :definition "A planting of pine trees"@en ;
  :synonym "pinery"@en .

:V_HTH a owl:Class ; 
  rdfs:subClassOf :V_SCRB ; 
  rdfs:label "heath"@en ; 
  :definition "An upland moor or sandy area dominated by low shrubby vegetation including heather"@en ;
  :synonym "heathland"@en, "moorland"@en, "moor"@en .

:V_MDW a owl:Class ; 
  rdfs:subClassOf :V_GRSLD ; 
  rdfs:label "meadow"@en ; 
  :definition "A small, poorly drained area dominated by grassy vegetation"@en . 

:V_OCH a owl:Class ; 
  rdfs:subClassOf :VegetationFeature, :AnthropogenicFeature ; 
  rdfs:label "orchard"@en ; 
  :definition "A planting of fruit or nut trees"@en ;
  :synonym "woodlet"@en .

:V_SCRB a owl:Class ; 
  rdfs:subClassOf :VegetationFeature ; 
  rdfs:label "scrubland"@en ; 
  :definition "An area of low trees, bushes, and shrubs stunted by some environmental limitation"@en ;
  :synonym "bushland"@en, "shrubland"@en .

:V_TREE a owl:Class ; 
  rdfs:subClassOf :VegetationFeature ; 
  rdfs:label "tree"@en ; 
  :definition "A conspicuous tree used as a landmark"@en . 

:V_TUND a owl:Class ; 
  rdfs:subClassOf :VegetationFeature ; 
  rdfs:label "tundra"@en ; 
  :definition "A marshy, treeless, high latitude plain, dominated by mosses, lichens, and low shrub vegetation under permafrost conditions"@en . 

:V_VIN a owl:Class ; 
  rdfs:subClassOf :V_OCH, :AreaFeature, :AnthropogenicFeature ; 
  rdfs:label "vineyard"@en ; 
  :definition "A planting of grapevines"@en ;
  :synonym "vinery"@en, "winegrowing"@en, "viticultural area"@en .


