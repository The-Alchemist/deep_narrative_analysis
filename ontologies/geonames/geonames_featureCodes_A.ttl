########################################################################
# Classes
########################################################################

:A_ADMD a owl:Class ; 
  rdfs:subClassOf :GeopoliticalEntity ; 
  rdfs:label "administrative division"@en ; 
  :definition "An administrative division of a country"@en ;
  :synonym "oblast"@en .

:A_ADM1 a owl:Class ; 
  rdfs:subClassOf :A_ADMD ; 
  rdfs:label "first-order administrative division"@en ; 
  :definition "A primary administrative division of a country, such as a state in the United States"@en . 

:A_ADM1H a owl:Class ; 
  rdfs:subClassOf :A_ADM1 ; 
  rdfs:label "historical first-order administrative division"@en ; 
  :definition "A former first-order administrative division"@en . 

:A_ADM2 a owl:Class ; 
  rdfs:subClassOf :A_ADMD ; 
  rdfs:label "second-order administrative division"@en ; 
  :definition "A subdivision of a first-order administrative division"@en . 

:A_ADM2H a owl:Class ; 
  rdfs:subClassOf :A_ADM2 ; 
  rdfs:label "historical second-order administrative division"@en ; 
  :definition "A former second-order administrative division"@en . 

:A_ADM3 a owl:Class ; 
  rdfs:subClassOf :A_ADMD ; 
  rdfs:label "third-order administrative division"@en ; 
  :definition "A subdivision of a second-order administrative division"@en . 

:A_ADM3H a owl:Class ; 
  rdfs:subClassOf :A_ADM3 ; 
  rdfs:label "historical third-order administrative division"@en ; 
  :definition "A former third-order administrative division"@en . 

:A_ADM4 a owl:Class ; 
  rdfs:subClassOf :A_ADMD ; 
  rdfs:label "fourth-order administrative division"@en ; 
  :definition "A subdivision of a third-order administrative division"@en . 

:A_ADM4H a owl:Class ; 
  rdfs:subClassOf :A_ADM4 ; 
  rdfs:label "historical fourth-order administrative division"@en ; 
  :definition "A former fourth-order administrative division"@en . 

:A_ADM5 a owl:Class ; 
  rdfs:subClassOf :A_ADMD ; 
  rdfs:label "fifth-order administrative division"@en ; 
  :definition "A subdivision of a fourth-order administrative division"@en . 

:A_ADM5H a owl:Class ; 
  rdfs:subClassOf :A_ADM5 ; 
  rdfs:label "historical fifth-order administrative division"@en ; 
  :definition "A former fifth-order administrative division"@en . 

:A_ADMDH a owl:Class ; 
  rdfs:subClassOf :A_ADMD ; 
  rdfs:label "historical administrative division "@en ; 
  :definition "A former administrative division of a political entity, undifferentiated as to administrative level"@en . 

:A_LTER a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "leased area"@en ; 
  :definition "A tract of land leased to another country, usually for military installations"@en . 

:A_PCLD a owl:Class ; 
  rdfs:subClassOf :GeopoliticalEntity ; 
  rdfs:label "dependent political entity"@en ; 
  :definition "A dependent political entity"@en . 

:A_PCLF a owl:Class ; 
  rdfs:subClassOf :GeopoliticalEntity ; 
  rdfs:label "freely associated state"@en ; 
  :definition "A freely associated state"@en . 

:A_PCLH a owl:Class ; 
  rdfs:subClassOf :GeopoliticalEntity ; 
  rdfs:label "historical political entity"@en ; 
  :definition "A former political entity"@en . 

:A_PCLI a owl:Class ; 
  rdfs:subClassOf :GeopoliticalEntity ; 
  rdfs:label "independent political entity"@en ; 
  :definition "An independent political entity"@en ;
  :synonym "politically autonomous"@en .

:A_PCLIX a owl:Class ; 
  rdfs:subClassOf :A_PCLI ; 
  rdfs:label "section of independent political entity"@en ; 
  :definition "A section of an independent political entity"@en . 

:A_PCLS a owl:Class ; 
  rdfs:subClassOf :GeopoliticalEntity ; 
  rdfs:label "semi-independent political entity"@en ; 
  :definition "A semi-independent political entity"@en . 

:A_PRSH a owl:Class ; 
  rdfs:subClassOf :GeopoliticalEntity ; 
  rdfs:label "parish"@en ; 
  :definition "An ecclesiastical district"@en .

:A_TERR a owl:Class ; 
  rdfs:subClassOf :GeopoliticalEntity ; 
  rdfs:label "territory"@en ; 
  :definition "A political territory"@en . 

:A_ZN a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "zone"@en ; 
  :definition "A political zone"@en ;
  :synonym "region"@en .

:A_ZNB a owl:Class ; 
  rdfs:subClassOf :A_ZN ; 
  rdfs:label "buffer zone"@en ; 
  :definition "A zone recognized as a buffer between two nations in which military presence is minimal or absent"@en ;
  :synonym "demilitarized"@en, "dmz"@en .


