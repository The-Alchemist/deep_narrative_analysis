@prefix : <urn:ontoinsights:ontology:dna:> .
@prefix dna: <urn:ontoinsights:ontology:dna:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

########################################################################
# File defining semantics of land areas
#    (GeoNames' 'L' feature codes)
# 
# Created: February 14, 2020
# Last modified: February 14, 2020
########################################################################


########################################################################
# Classes
########################################################################

:L_AGRC a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "agricultural colony"@en ; 
  :definition "A tract of land set aside for agricultural settlement"@en . 

:L_AMUS a owl:Class ; 
  rdfs:subClassOf :AreaFeature, :AnthropogenicFeature ; 
  rdfs:label "amusement park"@en ; 
  :definition "A theme or adventure park offering entertainment, similar to funfairs but with a fixed location"@en ;
  :synonym "theme park"@en .

:L_BSND a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "drainage basin"@en ; 
  :definition "An area drained by a stream"@en ;
  :synonym "catchment area"@en, "catchment basin"@en, "drainage area"@en .

:L_BSNP a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "petroleum basin"@en ; 
  :definition "An area underlain by an oil-rich structural basin"@en . 

:L_BTL a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "battlefield"@en ; 
  :definition "A site of a land battle of historical importance"@en ;
  :synonym "battleground"@en, "field of battle"@en .

:L_CLG a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "clearing"@en ; 
  :definition "An area in a forest with trees removed"@en . 

:L_CMN a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "common"@en ; 
  :definition "A park or pasture for community use"@en . 

:L_CNS a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "concession area"@en ; 
  :definition "A lease of land by a government for economic development, e.g., mining, forestry"@en ;
  :synonym "economic lease"@en .

:L_COLF a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "coalfield"@en ; 
  :definition "A region in which coal deposits of possible economic value occur"@en . 

:L_CONT a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "continent"@en ; 
  :definition "Continents are: Europe, Africa, Asia, North America, South America, Oceania, Antarctica"@en . 

:L_CST a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "coast"@en ; 
  :definition "A zone of variable width straddling the shoreline"@en ;
  :synonym "seashore"@en, "seacoast"@en, "coastline"@en, "shore"@en, "seaboard"@en, "shoreline"@en, "littoral"@en, "beach"@en .

:L_CTRB a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "business center"@en ; 
  :definition "A place where a number of businesses are located"@en . 

:L_DEVH a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "housing development"@en ; 
  :definition "A tract of land on which many houses of similar design are built according to a development plan"@en ;
  :synonym "subdivision"@en, "housing project"@en .

:L_FLD a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "field"@en ; 
  :definition "An open as opposed to wooded area"@en . 

:L_FLDI a owl:Class ; 
  rdfs:subClassOf :L_FLD ; 
  rdfs:label "irrigated field"@en ; 
  :definition "A tract of level or terraced land which is irrigated"@en . 

:L_GASF a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "gasfield"@en ; 
  :definition "An area containing a subterranean store of natural gas of economic value"@en . 

:L_GRAZ a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "grazing area"@en ; 
  :definition "An area of grasses and shrubs used for grazing"@en ;
  :synonym "rangeland"@en, "pastureland"@en, "pasture"@en .

:L_GVL a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "gravel area"@en ; 
  :definition "An area covered with gravel"@en . 

:L_INDS a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "industrial area"@en ; 
  :definition "An area characterized by industrial activity"@en . 

:L_LAND a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "arctic land"@en ; 
  :definition "A tract of land in the Arctic"@en ;
  :synonym "land of the midnight sun"@en .

:L_LCTY a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "locality"@en ; 
  :definition "A minor area or place of unspecified or mixed character and indefinite boundaries"@en ;
  :synonym "neighborhood"@en, "neighbourhood"@en .

:L_MILB a owl:Class ; 
  rdfs:subClassOf :AreaFeature, :S_INSM ; 
  rdfs:label "military base"@en ; 
  :definition "A place used by an army or other armed service for storing arms and supplies, and for accommodating and training troops, a base from which operations can be initiated"@en .

:L_MNA a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "mining area"@en ; 
  :definition "An area of mine sites where minerals and ores are extracted"@en . 

:L_MVA a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "maneuver area"@en ; 
  :definition "A tract of land where military field exercises are carried out"@en . 

:L_NVB a owl:Class ; 
  rdfs:subClassOf :AreaFeature, :S_INSM ; 
  rdfs:label "naval base"@en ; 
  :definition "An area used to store supplies, provide barracks for troops and naval personnel, a port for naval vessels, and from which operations are initiated"@en . 

:L_OAS a owl:Class ; 
  rdfs:subClassOf :AreaFeature, :T_DSRT ; 
  rdfs:label "oasis"@en ; 
  :definition "An area in a desert made productive by the availability of water"@en . 

:L_OILF a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "oilfield"@en ; 
  :definition "An area containing a subterranean store of petroleum of economic value"@en ;
  :synonym "oil field"@en .

:L_PEAT a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "peat cutting area"@en ; 
  :definition "An area where peat is harvested"@en . 

:L_PRK a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "park"@en ; 
  :definition "An area, often of forested land, maintained as a place of beauty, or for recreation"@en ;
  :synonym "parkland"@en .

:L_PRT a owl:Class ; 
  rdfs:subClassOf :AreaFeature, :TravelTransportRouteAndInfrastructure ; 
  rdfs:label "port"@en ; 
  :definition "A place provided with facilities for loading and discharging waterborne cargo or passengers, usually located in a harbor"@en . 

:L_QCKS a owl:Class ; 
  rdfs:subClassOf :AreaFeature, :T_SAND ; 
  rdfs:label "quicksand"@en ; 
  :definition "An area where loose sand with water moving through it may become unstable when heavy objects are placed at the surface, causing them to sink"@en . 

:L_RES a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "reserve"@en ; 
  :definition "A tract of public land reserved for future use or restricted as to use"@en ;
  :synonym "earmarked land"@en, "reserved land"@en .

:L_RESA a owl:Class ; 
  rdfs:subClassOf :L_RES ; 
  rdfs:label "agricultural reserve"@en ; 
  :definition "A tract of land reserved for agricultural reclamation and/or development"@en . 

:L_RESF a owl:Class ; 
  rdfs:subClassOf :L_RES ; 
  rdfs:label "forest reserve"@en ; 
  :definition "A forested area set aside for preservation or controlled use"@en . 

:L_RESH a owl:Class ; 
  rdfs:subClassOf :L_RES ; 
  rdfs:label "hunting reserve"@en ; 
  :definition "A tract of land used primarily for hunting"@en ;
  :synonym "game reserve"@en, "game park"@en .

:L_RESN a owl:Class ; 
  rdfs:subClassOf :L_RES ; 
  rdfs:label "nature reserve"@en ; 
  :definition "An area reserved for the maintenance of a natural habitat, and/or to protect and preserve its flora and fauna"@en ;
  :synonym "natural reserve"@en, "wildlife preserve"@en, "wildlife sanctuary"@en, "wildlife reserve"@en .

:L_RESV a owl:Class ; 
  rdfs:subClassOf :L_RES ; 
  rdfs:label "reservation"@en ; 
  :definition "A tract of land set aside for aboriginal, tribal, or native populations"@en . 

:L_RGN a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "region"@en ; 
  :definition "An area distinguished by one or more observable physical or cultural characteristics"@en . 

:L_RGNE a owl:Class ; 
  rdfs:subClassOf :L_RGN ; 
  rdfs:label "economic region"@en ; 
  :definition "A region of a country established for economic development or for statistical purposes"@en . 

:L_RGNH a owl:Class ; 
  rdfs:subClassOf :L_RGN ; 
  rdfs:label "historical region"@en ; 
  :definition "A former historic area distinguished by one or more observable physical or cultural characteristics"@en . 

:L_RGNL a owl:Class ; 
  rdfs:subClassOf :L_RGN ; 
  rdfs:label "lake region"@en ; 
  :definition "A tract of land distinguished by numerous lakes"@en . 

:L_RNGA a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "artillery range"@en ; 
  :definition "A tract of land used for artillery firing practice"@en . 

:L_SALT a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "salt area"@en ; 
  :definition "A shallow basin or flat where salt accumulates after periodic inundation"@en ;
  :synonym "salt flat"@en .

:L_SNOW a owl:Class ; 
  rdfs:subClassOf :AreaFeature, :H_GLCR ; 
  rdfs:label "snowfield"@en ; 
  :definition "An area of permanent snow and ice forming the accumulation area of a glacier"@en . 

:L_TRB a owl:Class ; 
  rdfs:subClassOf :AreaFeature ; 
  rdfs:label "tribal area"@en ; 
  :definition "A tract of land used by nomadic or other tribes"@en .
