@prefix : <urn:ontoinsights:dna:> .
@prefix dna: <urn:ontoinsights:dna:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

########################################################################
# File defining semantics related to specific events/actions/occurrences.
# 
# The subclasses of Event should be extended as needed for a domain
# of interest.
# 
# Created: April 1 2021
# Last Modified: June 10 2021
# 
# Licensed by OntoInsights, LLC
# Creative Commons Atribution 4.0 International (CC BY 4.0)
# 
# Unless required by applicable law or agreed to in writing,
# software distributed under the License is distributed on an
# "AS IS" BASIS, WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND,
# either express or implied.
# 
# Changes:
#  Added MistakeAndRegret, MurderAndHomicide
#  Updated the default and dna prefixes
#  Added multiple new classes and VerbNet synonyms
#  Renamed :value to :reported_numeric and added :reported_string
########################################################################


########################################################################
# Classes
########################################################################

:Abduction a owl:Class ;
  rdfs:subClassOf :AggressiveOrHostileAct ;
  rdfs:label "Abduction" ;
  :definition "An act of taking an Agent (a Person or GroupOfAgents/Persons) against their will." ;
  :synonym "kidnap", "hijack" .

:Acquisition a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Acquisition" ;
  :definition "The obtaining of some object/asset by any means. Subclasses indicate whether the Acquisition was a gift, purchase, recovered item, theft, etc." ;
  :synonym "equip", "obtain", "exchange" .

:AdviceAndRecommendation a owl:Class ;
  rdfs:subClassOf :CommunicationAndSpeechAct ;
  rdfs:label "Advice and Recommendation" ;
  :definition "An act of recommending or suggesting something. The Agent doing the communicating is indicated using the :has_active_agent property, while the entity being advised is defined using :has_affected_agent." ;
  :synonym "guidance" .

:Addiction a owl:Class ;
  rdfs:subClassOf :HealthAndDiseaseRelated ;
  rdfs:label "Acquisition" ;
  :definition "The obtaining of some object/asset by any means. Subclasses indicate whether the Acquisition was a gift, purchase, recovered item, theft, etc." .

:AggressiveOrHostileAct a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Aggressive or Hostile Act" ;
  :definition "Unfriendly, offensive, damaging or belligerent actions or statements. Note that if the Event involved a statement/communication, then the individual should also be classified as a :CommunicationAndSpeechAct." ;
  :synonym "confrontation" .

:Agreement a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Agreement" ;
  :definition "Events where two or more Agents share the same opinion and/or make arrangements (perhaps legally) to do something. Note that the property, :has_topic, indicates 'what is agreed to' and the involved Agents are identified using the :has_active_agent predicate." ;
  :synonym "commitment", "acquiescence" .

:AgricultureApicultureAndAquaculture a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Agriculture, Apiculture and Aquaculture" ;
  :definition "Events dealing with farming, ranching, breeding, beekeeping, etc." .

:ArtAndEntertainmentEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Art and Entertainment Event" ;
  :definition "Events involved with the display and/or performance of art, music, films, plays, etc., as well as other occurrences such as sporting events. These activities are 'observed' by Agents for enjoyment (who are indicated using the :has_affected_agent predicate) and may be 'performed by' other Agents (specified using the :has_active_agent property)." ;
  :synonym "performance", "rehearsal" .

:AssessmentAndCharacterization a owl:Class ;
  rdfs:subClassOf :Cognition ;
  rdfs:label "Assessment and Characterization" ;
  :definition "Events dealing with evaluating, characterizing or estimating something or someone." ;
  :synonym "evaluation", "judgment", "estimation", "appraisal", "gauge", "comparison", "conjecture", "classification", "correlation", "deduction" .

:AttackDamageAndAssault a owl:Class ;
  rdfs:subClassOf :AggressiveOrHostileAct ;
  rdfs:label "Attack, Damage and Assault" ;
  :definition "An aggressive action taken against other Agent(s) and/or things and which is intended to cause harm or damage. If the action involved the use of weapons, these can identified using the :has_instrument property. The location of the attack or damage is defined using the :has_location property, while the attacking Agents are specified using the :has_active_agent predicate. The thing or Agent that is attacked is defined using the :has_affected_resource/:has_affected_agent predicates (respectively). Note that if the action involved a statement/communication, then the Event should also be classified as a :CommunicationAndSpeechAct." ;
  :synonym "destruction" .

:Attempt a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Attempt" ;
  :definition "Attempt should not be used alone to classify an individual. Another Event subclass should also be specified. Attempt indicates that an effort was made to do/complete the 'other' Event, but it did not succeed/was not accomplished." ;
  :synonym "try", "seek out to", "endeavor", "strive to".

:AttentionAndFocus a owl:Class ;
  rdfs:subClassOf :Cognition ;
  rdfs:label "Attention and Focus" ;
  :definition "The act of a Person paying close attention to/concentrating on something or someone. The entity who/which is concentrated on is indicated by the :has_topic property." ;
  :synonym "focus" .

:Avoidance a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Avoidance" ;
  :definition "The act of staying away from some object or Agent, purposefully not doing something or preventing something from occurring." ;
  :synonym "restraint", "refraining" .

:Belief a owl:Class ;
  rdfs:subClassOf :Cognition ;
  rdfs:label "Belief" ;
  :definition "An 'attitude that something is the case, or that some proposition about the world is true' (https://en.wikipedia.org/w/index.php?title=Belief&oldid=1013662709)." ;
  :synonym "trust", "confidence", "suspicion" .

:Birth a owl:Class ;
  rdfs:subClassOf :LifeEvent ;
  rdfs:label "Birth" ;
  :definition "The act of an individual (animal or Person) being born/beginning life." .

:BodilyAct a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Bodily Act" ;
  :definition "An activity typically involving the body of an animal or Person (such as coughing, eating or running)." ;
  :synonym "sleep" .

:BodyMovement a owl:Class ;
  rdfs:subClassOf :BodilyAct ;
  rdfs:label "Body Movement" ;
  :definition "An activity related to the movement of an animal's or Person's body (such as dancing or running)." ;
  :synonym "breathe", "curtsey", "bow", "flinch" . 

:CaptureAndSeizure a owl:Class ;
  rdfs:subClassOf :Acquisition, :AggressiveOrHostileAct ;
  rdfs:label "Capture and Seizure" ;
  :definition "An act involved taking possession or control of some Resource or some Agent, by force. The Agent who performs the act is specified using the :has_active_agent property, while the Agent which is taken, controlled, or otherwise affected by the act is identified using :has_affected_agent. If a thing/Resource is captured, that entity is indicated using the :has_topic predicate." ;
  :synonym "takeover", "annexation", "occupation", "overrunning", "arrest", "take captive", "take control" .

:Ceremony a owl:Class ;
  rdfs:subClassOf :MeetingAndEncounter ;
  rdfs:label "Ceremony" ;
  :definition "A gathering to formally/officially highlight an occurrence of importance (such as a wedding, funeral or the opening of a facility) or to remember a previous event (such as a birthday or historical Event). The occurrence being highlighted or remembered is identified using the :has_topic predicate." .

:Change a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Change" ;
  :definition "Indicates a difference in the availability/count/amount of something or in the demeanor/behavior of an Agent. The thing/Agent which 'changes' is referenced using the :has_topic property. Note that two subclasses are defined, Increase and Decrease." ;
  :example "Examples are changes in the amount of food, water or schools, an increase/decrease in the number of certain types of Events (such as murders) or available teachers (a GroupOfAgents with the :has_line_of_business predicate set to :enum:Education and the :prototypical property set to true), etc." ;
  :synonym "conversion", "adjustment" .

:CoercionAndIntimidation a owl:Class ;
  rdfs:subClassOf :AggressiveOrHostileAct ;
  rdfs:label "Coercion and Intimidation" ;
  :definition "An action involving the use of force and/or authority to compel or prevent action, and/or to induce fear." ;
  :synonym "force" .

:Cognition a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Cognition" ;
  :definition "The 'mental action or process of acquiring knowledge and understanding through thought, experience, and the senses' (https://www.lexico.com/definition/cognition). Note that the Person who is acquiring this knowledge and understanding is defined using the :has_active_agent property. And, details about what is learned/known/understood is indicated using the :has_topic predicate." .

:Command a owl:Class ;
  rdfs:subClassOf :RequestAndAppeal ;
  rdfs:label "Command" ;
  :definition "A communication that issues orders, instructions, etc." ;
  :synonym "order" .

:CommunicationAndSpeechAct a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Communication and Speech Act" ;
  :definition "Any act of conveying information via written or spoken word, or non-verbal means. The Agent doing the communicating is indicated using the :has_active_agent property, while the entity listening is defined using :has_affected_agent." ;
  :synonym "declaration", "statement", "tell", "talk", "say", "lecture" .

:CompetitiveEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Competitive Event" ;
  :definition "An Event where two or more Agents are striving for a similar goal, and want to be better/more successful than the others. This class refers to a specific competitive Event, as opposed to a general condition (:Competition). The former would be Evidence for the latter." .

:Complaint a owl:Class ;
  rdfs:subClassOf :CommunicationAndSpeechAct ;
  rdfs:label "Complaint" ;
  :definition "An expression of displeasure or annoyance about something. The Agent doing the complaining is indicated using the :has_active_agent property, while the thing complained about is defined using :has_topic." ;
  :synonym "grumble", "whine" .

:Concealment a owl:Class ;
  rdfs:subClassOf :Prevention ;
  rdfs:label "Concealment" ;
  :definition "An act of preventing something/someone from being known/found." ;
  :synonym "hide", "conceal" .

:Confession a owl:Class ;
  rdfs:subClassOf :CommunicationAndSpeechAct ;
  rdfs:label "Confession" ;
  :definition "An admission of guilt or responsibility for some act. The Agent doing the confessing is indicated using the :has_active_agent property, while the thing that is confessed is defined using :has_topic." ;
  :synonym "admission", "taking responsibility" .

:CooperativeEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Cooperative Event" ;
  :definition "An Event where two or more Agents are striving for a similar goal, and are mutually assisting/supporting one another. This class refers to a specific coooperative Event, as opposed to a general condition (:Cooperation). The former would be Evidence for the latter." ;
  :synonym "aiding", "helping" .

:Cutting a owl:Class ;
  rdfs:subClassOf :Separation ;
  rdfs:label "Cutting" ;
  :definition "The act of making an opening or incision in something or someone." ;
  :synonym "laceration", "slit", "slash", "carve", "calve", "cut" .

:Death a owl:Class ;
  rdfs:subClassOf :LifeEvent ;
  rdfs:label "Death" ;
  :definition "The act of an individual (animal or Person) ending life." .

:DecisionMaking a owl:Class ;
  rdfs:subClassOf :Cognition ;
  rdfs:label "Decision Making" ;
  :definition "An act of concluding or resolving some issue or question." ;
  :synonym "consideration" .

:Decrease a owl:Class ;
  rdfs:subClassOf :Change ;
  rdfs:label "Decrease" ;
  :definition "Indicates a lowering/decline in the availability/count/amount of something or in the demeanor/behavior of an Agent." ;
  :synonym "reduction", "drop", "lessen", "lower", "decline" .

:DefensiveOrProtectiveAct a owl:Class ;
  rdfs:subClassOf :FriendlyOrSupportiveAct ;
  rdfs:label "Defensive or Protective Act" ;
  :definition "An action intended to prevent or lessen harm or damage, or to protect from aggression. If the action involved the use of weapons, these can identified using the :has_instrument property. Note that if the action involved a statement/communication, then the Event should also be classified as a :CommunicationAndSpeechAct." ;
  :synonym "prevention", "preservation", "conservation" .

:DelayAndWait a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Delay and Wait" ;
  :definition "A lack of action." .

:Demand a owl:Class ;
  rdfs:subClassOf :RequestAndAppeal ;
  rdfs:label "Demand" ;
  :definition "A communication insisting on immediate attention." .

:Desire a owl:Class ;
  rdfs:subClassOf :Cognition ;
  rdfs:label "Desire" ;
  :definition "Wanting something, someone or some Event or StateAndCondition to occur. The thing, EventAndState or Agent that is 'wanted' is specified using the :has_topic property." ;
  :synonym "wanting", "longing", "wish" .

:DisagreementOrDispute a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Disagreement or Dispute" ;
  :definition "Events where two or more Agents do NOT share the same opinion and argue. Note that the property, :has_topic, indicates 'what is in disagreement' and the involved Agents are identified using the :has_active_agent predicate." .

:Disaster a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Disaster" ;
  :definition "An Event (either natural or human-made) with 'serious disruption occurring over a short or long period of time that causes widespread human, material, economic or environmental loss which exceeds the ability of the affected [Agent], community or society to cope using its own resources' (https://en.wikipedia.org/w/index.php?title=Disaster&oldid=1013917618)." ;
  :synonym "catastrophe", "cataclysm", "calamity", "act of God" .

:Eating a owl:Class ;
  rdfs:subClassOf :BodilyAct ;
  rdfs:label "Eating" ;
  :definition "An activity related to an animal's or Person's intake of food/nutrients." ;
  :synonym "chewing", "dining", "feeding" .

:EducationEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Education Event" ;
  :definition "An Event involved with teaching such as the hiring of teachers or a new approach to testing." .

:Election a owl:Class ;
  rdfs:subClassOf :PoliticalEvent ;
  rdfs:label "Election" ;
  :definition "The act of choosing an Agent to play a role or to hold a position within a GroupOfAgents." ;
  :example "Examples are the election of a Person to political office, or to serve in a role such as a jury foreman or chairman of a committee." .

:EmotionalResponse a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Emotional Response" ;
  :definition "A state of mind associated with positive/pleasurable or negative/displeasurable feelings, thoughts and moods." .

:EmploymentEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Employment Event" ;
  :definition "An Event associated with a business and its workers. Note that EmploymentEvent is not the specific affiliation of an Agent in a business/organization, but is about occurrences related to hiring, firing, lay offs, increase/decrease in pay, etc." .

:Enforcement a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Enforcement" ;
  :definition "An act that compels/insists on obedience or compliance to a law or obligation." .

:EnvironmentalOrEcologicalEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Environmental or Ecological Event" ;
  :definition "An occurrence in some geographic region that affects the natural world and the habitats of (and relationships between) plants and animals." .

:EscapeAndEvasion a owl:Class ;
  rdfs:subClassOf :Avoidance ;
  rdfs:label "Escape and Evasion" ;
  :definition "An act of avoiding/getting away from something, someone or some Event, or of breaking free from captivity or oppression." .

:FinancialEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Financial Event" ;
  :definition "Events dealing with monies/currencies and funds and other capital." .

:FriendlyOrSupportiveAct a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Friendly or Supportive Act" ;
  :definition "Favorable, encouraging or helpful actions or statements. Note that if the Event involved a statement/communication, then the individual should also be classified as a :CommunicationAndSpeechAct." .

:GamblingAndWagering a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Gambling and Wagering" ;
  :definition "An Event where an Agent takes a risk (or performs a risky action) in the hopes of success/reward/a desired result." ;
  :synonym "betting" .

:GiftAndDonation a owl:Class ;
  rdfs:subClassOf :Acquisition ;
  rdfs:label "Gift and Donation" ;
  :definition "An act of giving with no expectation of payment." ;
  :synonym "contribution", "present" , "give" .

:Grooming a owl:Class ;
  rdfs:subClassOf :BodilyAct ;
  rdfs:label "Grooming" ;
  :definition "An activity related to the cleaning, brushing, etc. of an animal's or Person's body." ;
  :synonym "dressing", "combing", "brushing" .

:IgnoringAndOverlooking a owl:Class ;
  rdfs:subClassOf :Avoidance, :Cognition ;
  rdfs:label "Ignoring and Overlooking" ;
  :definition "An act of purposefully disregarding, not noticing or not acknowledging something or someone." ;
  :synonym "disregard", "neglect" .

:ImitationAndCounterfeit a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Imitation and Counterfeit" ;
  :definition "An act of simulating or making a copy or duplicate of something or someone. If done to deceive, then this is counterfeiting." ;
  :synonym "copy", "duplicate", "simulation", "reproduction" .

:ImpactAndContact a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Impact and Contact" ;
  :definition "An act of coming into physical contact/touching, which may be forceful (e.g., an impact)" ;
  :synonym "touch", "hit", "swat", "bump", "poke" .

:InclusionAttachmentAndUnification a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Inclusion, Attachment and Unification" ;
  :definition "An act of fastening, joining, grouping or bringing together, etc." ;
  :synonym "amalgamation", "combination" .

:Increase a owl:Class ;
  rdfs:subClassOf :Change ;
  rdfs:label "Increase" ;
  :definition "Indicates a growth/rise in the availability/count/amount of something or in the demeanor/behavior of an Agent." ;
  :synonym "growth", "rise", "expansion", "escalation" .

:InformationHandling a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Information Handling" ;
  :definition "An activity involved with the collection, manipulation, storage, transmission, protection or disruption of information, especially digital information and cyber activities." .

:InProcessPlacement a owl:Class ;
  rdfs:subClassOf :MovementTravelAndTransportation ;
  rdfs:label "In-Process Placement" ;
  :definition "Any act that changes the location of something by incrremental amounts or makes a small change to the placement of something, such as putting an object on a table, pouring a liquid into a container, etc. The Agent which moves the thing(s) is specified using the :has_active_agent predicate, while the entity(ies) that are moved are identified using the :has_topic property." ;
  :synonym "put", "funnel", "pour", "coil", "spray", "fill", "pocket", "slide", "push", "throw", "run", "roll" .

:Inquiry a owl:Class ;
  rdfs:subClassOf :CommunicationAndSpeechAct ;
  rdfs:label "Inquiry" ;
  :definition "A CommunicationAndSpeechAct involving asking questions/for information." ;
  :synonym "interrogation" .

:Intention a owl:Class ;
  rdfs:subClassOf :Cognition ;
  rdfs:label "Intention" ;
  :definition "The Event or StateAndCondition that an Agent hopes to attain/accomplish." ;
  :synonym "goal", "aim", "objective" .

:KnowledgeAndSkill a owl:Class ;
  rdfs:subClassOf :Cognition ;
  rdfs:label "Knowledge and Skill" ;
  :definition "Involved with 'a familiarity, awareness, or understanding of someone or something, such as facts (descriptive knowledge), skills (procedural knowledge), or objects (acquaintance knowledge). By most accounts, knowledge can be acquired in many different ways and from many sources, including but not limited to perception, reason, memory, testimony, scientific inquiry, education, and practice.' (https://en.wikipedia.org/w/index.php?title=Knowledge&oldid=1014835309)" ;
  :synonym "understanding", "comprehension", "awareness" .

:Learning a owl:Class ;
  rdfs:subClassOf :Acquisition, :KnowledgeAndSkill;
  rdfs:label "Learning" ;
  :definition "An act of acquiring knowledge and skills through study, repetition, experience, instruction, etc." ;
  :synonym "study", "education", "schooling" .

:LegalEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Legal Event" ;
  :definition "Any activity based on following legal practices, or related to or allowed by laws or the legal system." .

:LifeEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Life Event" ;
  :definition "Any occurrence in a living entity's and Person's life, such as birth, death, marriage, divorce, etc." .

:Measurement a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Measurement" ;
  :definition "The act of observing and reporting on properties such as length, size, amount, temperature, pressure, etc. at a specific time and location (as identified by the :has_time and :has_location properties). If the measurement is made by an instrument (such as a sensor), that can be specified using the :has_instrument property." ;
  :synonym "quantification" .

:MeetingAndEncounter a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Meeting and Encounter" ;
  :definition "Any Event that is a formal, informal or coincidental gathering or assembly of people." ;
  :synonym "gathering", "assembly", "conference" , "social event", "herding", "swarming" .

:Memory a owl:Class ;
  rdfs:subClassOf :Cognition ;
  rdfs:label "Memory" ;
  :definition "An act of Cognition dealing with the recollection/retrieval of past occurrences, learning and insights." ;
  :synonym "recollection", "remembrance", "reminiscence" .

:MistakeAndRegret a owl:Class ;
  rdfs:subClassOf :AssessmentAndCharacterization ;
  rdfs:label "Mistake and Regret" ;
  :definition "An act of Cognition/judgment indicating that a previous Event is believed to be wrong or faulty." ;
  :synonym "error", "fault", "inaccuracy", "omission", "miscalculation" .

:MovementTravelAndTransportation a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Movement, Travel and Transportation" ;
  :definition "Any act that changes the location of something or someone. If movement is accomplished using a vehicle (such as an airplane, train, ship or truck), that mode of transport is indicated using the :has_instrument property. The Agent which moves the things or people is specified using the :has_active_agent predicate, while the entity that is moved is identified using the :has_topic property. And, to identify the Agents sending/receiving the entities or their origin/destination locations, the properties, :has_provider/recipient and :has_origin/destination (respectively), should be used." ;
  :synonym "send", "bring", "drive", "leave", "arrival", "departure" .

:MurderAndHomicide a owl:Class ;
  rdfs:subClassOf :AttackDamageAndAssault ;
  rdfs:label "Murder and Homicide" ;
  :definition "Any Event that is related to killing a person or living thing." ;
  :synonym "killing", "extermination", "assassination" , "execution", "slaughter", "massacre", "liquidation" .

:NegativeEmotion a owl:Class ;
  rdfs:subClassOf :EmotionalResponse ;
  rdfs:label "Negative Emotion" ;
  :definition "Any EmotionalResponse related to unpleasant, pessimistic or insecure feelings." ;
  :example "Examples are unhappiness, fear, frustration, loneliness, disgust, hatred, etc." .

:Pain a owl:Class ;
  rdfs:subClassOf :PhysicalSensation ;
  rdfs:label "Pain" ;
  :definition "A reaction of suffering or discomfort." ;
  :synonym "agony", "suffering", "soreness", "discomfort", "hurt" .

:Permission a owl:Class ;
  rdfs:subClassOf :CommunicationAndSpeechAct ;
  rdfs:label "Permission" ;
  :definition "An act of allowing or authorizing something to occur. The thing that is allowed is indicated using the :has_topic property." ;
  :synonym "authorization", "consent", "allowing", "approval" .

:PhysicalSensation a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Physical Sensation" ;
  :definition "The reaction of a body to something that happens to it, comes in contact with it or that is lacking." ;
  :example "Examples are burns and pains due to hunger or injury." .

:PoliticalEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Political Event" ;
  :definition "Any Event related to the governance, government and public affairs of a community or country." .

:PositiveEmotion a owl:Class ;
  rdfs:subClassOf :EmotionalResponse ;
  rdfs:label "Positive Emotion" ;
  :definition "Any EmotionalResponse related to constructive, optimistic or confident feelings." ;
  :example "Examples are happiness, love, forgiveness, hope, gratitude, etc." ;
  :synonym "admiration", "amusement", "marveling", "caring", "empathy" .

:Prevention a owl:Class ;
  rdfs:subClassOf :Avoidance ;
  rdfs:label "Prevention" ;
  :definition "An act of stopping some Event or StateAndCondition from occurring. Note that the event/state/condition is identified using the :has_topic property" ;
  :synonym "precaution", "safeguarding" .

:ProductionManufactureAndCreation a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Production, Manufacture and Creation" ;
  :definition "Events involved with defining, supervising and engaging in the construction of some entity. The entity that is manufactured/created/produced is defined using the :has_topic predicate." ;
  :synonym "build", "construction", "assembly", "construct", "grow" .

:Protest a owl:Class ;
  rdfs:subClassOf :AggressiveOrHostileAct ;
  rdfs:label "Protest" ;
  :definition "An act (such as a CommunicationAndSpeechAct, or gathering/MeetingAndEncounter) indicating disapproval or objection to something or someone. Other Event subclasses (such as MeetingAndEncounter) should be added to more fully define the Event. The entity which is disapproved of/object to is defined using the :has_topic property." ;
  :synonym "objection", "complaint", "disapproval" .

:Publication a owl:Class ;
  rdfs:subClassOf :ReturnAndRelease ;
  rdfs:label "Publication" ;
  :definition "An act of issuing/releasing an InformationSource such as a book, article, newspaper, etc., a work of art or music, ..." .

:Punishment a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Punishment" ;
  :definition "An act of penalizing an Agent because of some occurrence/offense. The Event which is the (perceived) offense is identified using the :has_topic predicate. The Agent (e.g., an Animal, Person or GroupOfAgents) that is punished is indicated using the :has_affected_agent property. And, the details of the punishment are defined using the :has_instrument property." ;
  :synonym "discipline", "retribution" .

:PurchaseAndSale a owl:Class ;
  rdfs:subClassOf :Acquisition, :FinancialEvent ;
  rdfs:label "Purchase and Sale" ;
  :definition "An act of Acquisition/transfer of ownership of something by the exchange of money, assets, etc. The 'buyer' is identified using the property, :has_active_agent, while the 'seller' is identified using the :has_affected_agent predicate. The details of what monies, assets, etc. are used are expressed using the :has_instrument property, while the thing sold is defined using the :has_topic property." ;
  :synonym "buying", "selling", "procurement", "vending", "paying" .

:RecoveryAndRescue a owl:Class ;
  rdfs:subClassOf :Acquisition ;
  rdfs:label "Recovery and Rescue" ;
  :definition "An act of returning something or someone who/which was lost, taken or in a perilous situation." ;
  :synonym "retrieval", "saving" .

:RecreationEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Recreation Event" ;
  :definition "Any activity done for relaxation and enjoyment (not an Agent's job or business)." ;
  :synonym "leisure", "relaxation" .

:RefusalAndRejection a owl:Class ;
  rdfs:subClassOf :CommunicationAndSpeechAct ;
  rdfs:label "Refusal and Rejection" ;
  :definition "An act related to not accepting or acknowledging something, someone, or some Event or StateAndCondition. The entity that is not acknowledged/accepted is identified using the :has_topic predicate." ;
  :synonym "denial", "nonacceptance", "forbidding" .

:RemovalAndRestriction a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Removal and Restriction" ;
  :definition "An act intended to decrease, reduce, take away, dispose of or discard something or someone. What is removed/restricted is identified using the :has_topic property. The thing or Agent that is affected by the removal/restriction is defined using the :has_affected_resource/:has_affected_agent properties (respectively)." ;
  :synonym "limitation", "cutback", "sanction" , "banishment", "clear", "wipe", "extraction", "withdrawal" .

:RemovalOfRawMaterials a owl:Class ;
  rdfs:subClassOf :RemovalAndRestriction ;
  rdfs:label "Removal of Raw Materials (Mining, Drilling, Logging, ...)" ;
  :definition "A removal or reduction in raw materials due to mining, logging, fishing, drilling and similar activities" ;
  :synonym "mining", "logging" .

:Repair a owl:Class ;
  rdfs:subClassOf :FriendlyOrSupportiveAct ;
  rdfs:label "Repair" ;
  :definition "Any act that fixes/mends/adjusts/restores an entity that is broken, damaged or faulty." ;
  :synonym "restoration", "mend", "fix", "renovation" .

:ReportedValue a owl:Class ;
  rdfs:label "Reported Value" ;
  :definition "A reification of a measurement/measured quantity - where its properties, :reported_numeric and :reported_string, hold the decimal or string value that is 'reported', and :has_units defines the units (such as seconds or meters) in which the value is expressed." .

:RequestAndAppeal a owl:Class ;
  rdfs:subClassOf :CommunicationAndSpeechAct ;
  rdfs:label "Request and Appeal" ;
  :definition "A CommunicationAndSpeechAct that expresses a need or desire for something or someone, or that some Event occur. Note that who/what is requested is identified using the :has_topic predicate." ;
  :synonym "entreaty", "asking for", "plea", "petition", "solicitation", "supplication", "urge", "beg" .

:Resistance a owl:Class ;
  rdfs:subClassOf :AggressiveOrHostileAct ;
  rdfs:label "Resistance" ;
  :definition "An act of opposition to someone or something, or a refusal to accept or comply with something. The entity that is being resisted is identified using the :has_topic predicate." ;
  :synonym "opposition" .

:ReturnAndRelease a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Return and Release" ;
  :definition "An act of restoring something or someone to their original owner or location, granting freedom to someone or something, or letting something or someone fall/spill/drop. Note that the entity that is returned/released can be described using the :has_topic predicate, while the original (owning) Agent or location can be specified using the :has_affected_agent and :has_origin properties (respectively)." ;
  :synonym "free" .

:Revelation a owl:Class ;
  rdfs:subClassOf :Acquisition ;
  rdfs:label "Revelation" ;
  :definition "An act of exposing/making something/someone known." ;
  :synonym "disclosure", "divulge" .

:Reward a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Reward" ;
  :definition "An act of giving something or someone to an Agent to recognize their services, achievements/accomplishments or efforts. If a specific Event or StateAndCondition (such as affiliation with an Organization) is being recognized, that is identified using the :has_topic predicate. The Agent (e.g., an Animal, Person or GroupOfAgents) that is rewarded is indicated using the :has_affected_agent property. And, the details of the reward are defined using the :has_instrument property." ;
  :synonym "recompense", "prize", "winnings", "award" .

:RiskTaking a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Risk Taking" ;
  :definition "Proceeding with an act that is dangerous and/or may result in injury, failure or loss." ;
  :synonym "audacity", "bravado" .

:RobberyAndTheft a owl:Class ;
  rdfs:subClassOf :CaptureAndSeizure ;
  rdfs:label "Robbery and Theft" ;
  :definition "An act involved with taking something unlawfully, from an Agent. The Agent who is the thief is specified using the :has_active_agent property, while the Agent stolen from is identified using :has_affected_agent. The thing(s) taken are indicated using the :has_topic predicate." ;
  :synonym "stealing", "burglary", "thievery", "breaking and entering" .

:Searching a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Searching" ;
  :definition "An act of looking for something or someone. The entity that is searched for is identified using the :has_topic property." ;
  :synonym "hunt", "look for", "exploration", "forage", "stalk", "rummage" .

:SensoryPerception a owl:Class ;
  rdfs:subClassOf :Cognition ;
  rdfs:label "Sensory Perception" ;
  :definition "An act involving one of the senses - sight, hearing, touch, taste and smell." ;
  :synonym "sight", "smell", "taste", "touch", "hearing" .

:Separation a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Separation" ;
  :definition "An act of moving two or more things or Agents some distance from each other, or of breaking an entity into its parts." ;
  :synonym "disconnection", "detachment", "severance", "uncoupling", "dissociation", "splitting", "disassembly", "breakage" .

:SpaceEvent a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Space Event" ;
  :definition "Any event that occurs beyond the Earth." ;
  :example "Examples are comet, asteroid or meteoroid impacts, meteor showers, burn-up of space debris in the atmosphere, solar flares, eclipses, etc." .

:Storage a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Storage" ;
  :definition "Any act of keeping something for future delivery, sale or use." ;
  :synonym "warehousing", "retention" .

:Substitution a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Substitution" ;
  :definition "An act of replacing something or someone with another." ;
  :synonym "exchange", "interchange", "replacement", "swap" .

:Suicide a owl:Class ;
  rdfs:subClassOf :Death ;
  rdfs:label "Suicide" ;
  :definition "The act of an individual (animal or Person) taking their own life." .

:SurrenderAndYielding a owl:Class ;
  rdfs:subClassOf :CommunicationAndSpeechAct ;
  rdfs:label "Surrender and Yielding" ;
  :definition "An act where one Agent admits defeat and/or agrees to give up fighting or to relinquish power, possession or control of something to another. Note that this Event may involve making a statement or issuing a communication, if so, the individual should also be classified as a :CommunicationAndSpeechAct." ;
  :synonym "capitulation", "succombing", "giving in" .

:Threat a owl:Class ;
  rdfs:subClassOf :CommunicationAndSpeechAct ;
  rdfs:label "Threat" ;
  :definition "A CommunicationAndSpeechAct intended to frighten by expressing the intention to take action in response to something (some Event) done or not done. The Event that is done/not done is specified using the :has_topic predicate. The Agent who is making the threats is defined using :has_active_agent, while the threatened Agent is identified using :has_affected_agent." ;
  :synonym "warning", "ultimatum", "menacing" .

:UtilizationAndConsumption a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "UtilizationAndConsumption" ;
  :definition "Any act that uses something or someone." .

:ViolationOfAgreement a owl:Class ;
  rdfs:subClassOf :Event ;
  rdfs:label "Violation of Agreement" ;
  :definition "Any act that disregards or goes against a previous Agreement or any of its conditions. The previous Agreement is indicated using the :has_topic property." .

:ViolenceAndFighting a owl:Class ;
  rdfs:subClassOf :AggressiveOrHostileAct ;
  rdfs:label "Violence and Fighting" ;
  :definition "Any act of aggression involving the use of physical force and/or weapons, and intended to injure, damage or cause harm." .

:WeatherEvent a owl:Class ;
  rdfs:subClassOf :EnvironmentalOrEcologicalEvent ;
  rdfs:label "Weather Event" ;
  :definition "Any environmental occurrence related to atmospheric conditions in an area." ;
  :example "Examples include high/low temperatures, high winds, precipitation, etc." .


########################################################################
# Disjointness
########################################################################

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :Acquisition :Avoidance ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :AggressiveOrHostileAct :FriendlyOrSupportiveAct ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :AttackDamageAndAssault :Repair ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :Agreement :DisagreementOrDispute ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :Agreement :ViolationOfAgreement ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :CooperativeEvent :CompetitiveEvent ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :Concealment :Revelation ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :InclusionAttachmentAndUnification :Separation ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :AttentionAndFocus :IgnoringAndOverlooking ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :MurderAndHomicide :Suicide ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :RemovalAndRestriction :ReturnAndRelease ) .

[] rdf:type owl:AllDisjointClasses ;
  owl:members  ( :Reward :Punishment ) .


########################################################################
# Datatype Properties
########################################################################

:reported_numeric a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "reported numeric value" ;
  :definition "Specifies the (decimal) value of a Measurement (reified as a ReportedValue, where the value's units are defined using the :has_unit predicate)." ;
  rdfs:domain :ReportedValue ;
  rdfs:range xsd:decimal .

:reported_string a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "reported string value" ;
  :definition "Specifies the (string) value of a Measurement (reified as a ReportedValue, where the value's units are defined using the :has_unit predicate)." ;
  rdfs:domain :ReportedValue ;
  rdfs:range xsd:string .


########################################################################
# Object Properties
########################################################################

:has_reported_value a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:label "has reported value" ;
  :definition "Associates a value and its units with a Measurement Event which defines its Location, Time and similar information." ;
  rdfs:domain :Measurement ;
  rdfs:range :ReportedValue .

:has_unit a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:label "has unit of measure" ;
  :definition "Associates the units of measure with a Measurement's value/ReportedValue. Units are defined by QUDT.org and can be found at http://qudt.org/2.1/vocab/unit." ;
  rdfs:domain :ReportedValue ;
  rdfs:range <http://qudt.org/schema/qudt/Unit> .
